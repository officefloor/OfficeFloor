<context>
	<office-floor>
		<managed-object-sources>
			<managed-object-source name="DEPENDENT_SOURCE"
				source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$DependencyManagedObject" />
				</properties>
				<managing-office name="OFFICE" />
			</managed-object-source>
			<managed-object-source name="SIMPLE_SOURCE"
				source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$SimpleManagedObject" />
				</properties>
				<managing-office name="OFFICE" />
			</managed-object-source>
		</managed-object-sources>
		<managed-objects>
			<managed-object name="DEPENDENT" scope="PROCESS">
				<managed-object-source name="DEPENDENT_SOURCE" />
				<dependency name="dependency">
					<managed-object name="SIMPLE" />
				</dependency>
			</managed-object>
			<managed-object name="SIMPLE" scope="PROCESS">
				<managed-object-source name="SIMPLE_SOURCE" />
			</managed-object>
		</managed-objects>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<object name="OBJECT_A">
					<managed-object name="DEPENDENT" />
				</object>
				<object name="OBJECT_B">
					<managed-object name="SIMPLE" />
				</object>
			</office>
		</offices>
	</office-floor>
	<office>
		<external-managed-objects>
			<external-managed-object name="OBJECT_A"
				object-type="${testcase}$DependencyManagedObject" object-scope="THREAD">
			</external-managed-object>
			<external-managed-object name="OBJECT_B"
				object-type="${testcase}$SimpleManagedObject" object-scope="THREAD">
			</external-managed-object>
		</external-managed-objects>
	</office>
</context>