<context>
	<office-floor>
		<teams>
			<team name="TEAM" source="ONE_PERSON" />
		</teams>
		<managed-object-sources>
			<managed-object-source
				name="MANAGED_OBJECT_SOURCE" source="CLASS">
				<properties>
					<property name="class.name"
						value="${testcase}$SimpleManagedObject" />
				</properties>
				<managing-office name="OFFICE" />
			</managed-object-source>
		</managed-object-sources>
		<managed-objects>
			<managed-object name="MANAGED_OBJECT" scope="PROCESS">
				<managed-object-source
					name="MANAGED_OBJECT_SOURCE" />
			</managed-object>
		</managed-objects>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<team name="OFFICE_TEAM">
					<office-floor-team name="TEAM" />
				</team>
				<object name="EXTERNAL_MANAGED_OBJECT">
					<managed-object name="MANAGED_OBJECT" />
				</object>
			</office>
		</offices>
	</office-floor>
	<office>
		<teams>
			<team name="OFFICE_TEAM" />
		</teams>
		<external-managed-objects>
			<external-managed-object
				name="EXTERNAL_MANAGED_OBJECT" object-type="java.sql.Connection"
				object-scope="PROCESS">
				<!-- <govern governance="GOVERNANCE" /> -->
			</external-managed-object>
		</external-managed-objects>
		<governances>
			<governance name="GOVERNANCE" source="CLASS">
				<properties>
					<property name="class.name"
						value="${testcase}$SimpleGovernance" />
				</properties>
				<team name="OFFICE_TEAM" />
			</governance>
		</governances>
	</office>
</context>