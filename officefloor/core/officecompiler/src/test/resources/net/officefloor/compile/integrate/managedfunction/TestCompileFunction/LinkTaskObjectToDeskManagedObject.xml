<context>
	<office-floor>
		<teams>
			<team name="TEAM" source="ONE_PERSON" />
		</teams>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<team name="OFFICE_TEAM">
					<office-floor-team name="TEAM" />
				</team>
			</office>
		</offices>
	</office-floor>
	<office>
		<teams>
			<team name="OFFICE_TEAM" />
		</teams>
		<sections>
			<section name="SECTION" source="SECTION" location="section">
				<responsibility name="RESPONSIBILITY">
					<team name="OFFICE_TEAM" />
				</responsibility>
			</section>
		</sections>
	</office>
	<section>
		<sub-sections>
			<sub-section name="DESK" source="DESK" location="desk" />
		</sub-sections>
	</section>
	<desk>
		<managed-object-sources>
			<managed-object-source name="MANAGED_OBJECT_SOURCE"
				source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileManagedObject" />
				</properties>
				<managing-office name="OFFICE" />
			</managed-object-source>
		</managed-object-sources>
		<managed-objects>
			<managed-object name="MANAGED_OBJECT" scope="THREAD">
				<managed-object-source name="MANAGED_OBJECT_SOURCE" />
			</managed-object>
		</managed-objects>
		<works>
			<work name="WORK" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileTaskWork" />
				</properties>
				<work-task name="objectTask">
					<task-object name="${testcase}$CompileManagedObject" parameter="false">
						<managed-object name="MANAGED_OBJECT" />
					</task-object>
				</work-task>
			</work>
		</works>
		<tasks>
			<task name="TASK" work="WORK" task="objectTask" />
		</tasks>
	</desk>
</context>