<context>
	<office-floor>
		<teams>
			<team name="TEAM" source="ONE_PERSON" />
		</teams>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<team name="OFFICE_TEAM">
					<office-floor-team name="TEAM" />
				</team>
			</office>
		</offices>
	</office-floor>
	<office>
		<teams>
			<team name="OFFICE_TEAM" />
		</teams>
		<starting>
			<start name="START">
				<flow section="SECTION" input="INPUT" />
			</start>
		</starting>
		<sections>
			<section name="SECTION" source="SECTION" location="section">
				<input name="INPUT" />
				<responsibility name="RESPONSIBILITY">
					<team name="OFFICE_TEAM" />
				</responsibility>
			</section>
		</sections>
	</office>
	<section>
		<function-namespaces>
			<function-namespace name="NAMESPACE" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileFunctionClass" />
				</properties>
				<managed-function name="simpleFunction" />
			</function-namespace>
		</function-namespaces>
		<functions>
			<function name="INPUT" function-namespace="NAMESPACE"
				managed-function="simpleFunction" public="true" />
		</functions>
	</section>
</context>