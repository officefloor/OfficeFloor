<office>

	<external-managed-objects>
		<external-managed-object name="EXTERNAL_MANAGED_OBJECT"
			object-type="java.sql.Connection" object-scope="PROCESS" x="100" y="101">
			<administer administrator="ADMINISTRATOR" order="1" />
			<govern governance="GOVERNANCE" />
		</external-managed-object>
	</external-managed-objects>

	<managed-object-sources>
		<managed-object-source name="MANAGED_OBJECT_SOURCE"
			source="net.example.ExampleManagedObjectSource" object-type="net.orm.Session"
			timeout="10" x="200" y="201">
			<properties>
				<property name="MO_ONE" value="VALUE_ONE" />
				<property name="MO_TWO" value="VALUE_TWO" />
			</properties>
			<input-dependency name="INPUT_DEPENDENCY_ONE"
				object-type="java.sql.Connection">
				<managed-object name="MANAGED_OBJECT_TWO" />
			</input-dependency>
			<input-dependency name="INPUT_DEPENDENCY_TWO"
				object-type="java.sql.Connection">
				<external-managed-object name="EXTERNAL_MANAGED_OBJECT" />
			</input-dependency>
			<flow name="FLOW" argument-type="java.lang.Integer">
				<section-input section="SECTION" input="INPUT" />
			</flow>
			<team name="MO_TEAM">
				<office-team name="TEAM" />
			</team>
		</managed-object-source>
	</managed-object-sources>

	<managed-objects>
		<managed-object name="MANAGED_OBJECT_ONE" scope="THREAD"
			x="300" y="301">
			<managed-object-source name="MANAGED_OBJECT_SOURCE" />
			<administer administrator="ADMINISTRATOR" order="1" />
			<govern governance="GOVERNANCE" />
			<dependency name="DEPENDENCY_ONE" object-type="java.sql.Connection">
				<managed-object name="MANAGED_OBJECT_TWO" />
			</dependency>
			<dependency name="DEPENDENCY_TWO" object-type="java.sql.Connection">
				<external-managed-object name="EXTERNAL_MANAGED_OBJECT" />
			</dependency>
		</managed-object>
		<managed-object name="MANAGED_OBJECT_TWO" scope="PROCESS"
			x="310" y="311">
			<managed-object-source name="MANAGED_OBJECT_SOURCE" />
		</managed-object>
	</managed-objects>

	<teams>
		<team name="TEAM" x="400" y="401" />
	</teams>

	<escalations>
		<escalation escalation-type="java.lang.Exception" x="500"
			y="501">
			<handler section="SECTION" input="INPUT" />
		</escalation>
	</escalations>

	<administrators>
		<administrator name="ADMINISTRATOR"
			source="net.example.ExampleAdministratorSource" scope="THREAD" x="600"
			y="601">
			<properties>
				<property name="ADMIN_ONE" value="VALUE_ONE" />
				<property name="ADMIN_TWO" value="VALUE_TWO" />
			</properties>
			<team name="TEAM" />
			<duty name="DUTY_ONE" />
			<duty name="DUTY_TWO" />
		</administrator>
	</administrators>

	<governances>
		<governance name="GOVERNANCE" source="net.example.ExampleGovernanceSource"
			x="700" y="701">
			<properties>
				<property name="GOV_ONE" value="VALUE_ONE" />
				<property name="GOV_TWO" value="VALUE_TWO" />
			</properties>
			<team name="TEAM" />
			<governance-area x="710" y="711" height="720" width="721" />
		</governance>
	</governances>
	
	<starting>
		<start name="START" x="800" y="801">
			<flow section="SECTION" input="INPUT" />
		</start>
	</starting>

	<sections>
		<section name="SECTION" source="net.example.ExampleSectionSource"
			location="SECTION_LOCATION" x="900" y="901">
			<properties>
				<property name="PROP_ONE" value="VALUE_ONE" />
				<property name="PROP_TWO" value="VALUE_TWO" />
			</properties>
			<input name="INPUT" parameter-type="java.lang.Integer" />
			<output name="OUTPUT_ONE" argument-type="java.lang.Float"
				escalation-only="false">
				<section-input section="SECTION_TARGET" input="INPUT" />
			</output>
			<output name="OUTPUT_TWO" argument-type="java.lang.Exception"
				escalation-only="true" />
			<object name="OBJECT_ONE" object-type="java.sql.Connection">
				<external-managed-object name="EXTERNAL_MANAGED_OBJECT" />
			</object>
			<object name="OBJECT_TWO" object-type="net.orm.Session">
				<managed-object name="MANAGED_OBJECT" />
			</object>
			<responsibility name="RESPONSIBILITY">
				<team name="TEAM" />
				<object name="RESPONSIBILITY_OBJECT" />
			</responsibility>
			<sub-section>
				<section-managed-object name="SECTION_MANAGED_OBJECT">
					<team name="MO_TEAM" />
					<govern governance="GOVERNANCE" />
				</section-managed-object>
				<task name="TASK">
					<pre-administer administrator="ADMINISTRATOR" duty="DUTY_ONE" />
					<govern governance="GOVERNANCE" />
				</task>
				<govern governance="GOVERNANCE" />
				<sub-section name="SUB_SECTION">
					<section-managed-object name="SUB_SECTION_MANAGED_OBJECT" />
					<task name="SUB_SECTION_TASK">
						<post-administer administrator="ADMINISTRATOR"
							duty="DUTY_TWO" />
						<govern governance="GOVERNANCE" />
					</task>
					<sub-section name="SUB_SUB_SECTION" />
				</sub-section>
			</sub-section>
		</section>
		<section name="SECTION_TARGET" source="net.example.ExampleSectionSource"
			location="SECTION_LOCATION" x="910" y="911">
			<input name="INPUT" parameter-type="java.lang.Float" />
		</section>
	</sections>

</office>