<context>
	<office-floor>
		<teams>
			<team name="TEAM" source="ONE_PERSON" />
		</teams>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<team name="OFFICE_TEAM">
					<office-floor-team name="TEAM" />
				</team>
			</office>
		</offices>
	</office-floor>
	<office>
		<teams>
			<team name="OFFICE_TEAM" />
		</teams>
		<suppliers>
			<supplier name="SUPPLIER"
				source="${testcase}$MockSupplierSource" />
		</suppliers>
		<governances>
			<governance name="GOVERNANCE" source="CLASS"
				auto-wire-extensions="true">
				<properties>
					<property name="class.name"
						value="${testcase}$SimpleGovernance" />
				</properties>
				<team name="OFFICE_TEAM" />
			</governance>
		</governances>
	</office>
</context>