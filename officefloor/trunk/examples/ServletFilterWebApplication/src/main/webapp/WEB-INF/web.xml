<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<filter>
		<filter-name>Example</filter-name>
		<filter-class>net.officefloor.example.servletfilterwebapplication.ExampleFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Example</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>Mock</servlet-name>
		<servlet-class>net.officefloor.example.servletfilterwebapplication.MockHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Mock</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

</web-app>