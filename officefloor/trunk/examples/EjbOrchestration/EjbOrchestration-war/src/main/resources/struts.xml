<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="default" extends="struts-default">
		<action name="initialState"
			class="net.officefloor.example.ejborchestration.InitialStateAction">
			<result>/initialState.jsp</result>
		</action>
		<action name=""
			class="net.officefloor.example.ejborchestration.IndexAction">
			<result>/index.jsp</result>
		</action>
		<action name="index"
			class="net.officefloor.example.ejborchestration.IndexAction">
			<result>/index.jsp</result>
		</action>
		<action name="login"
			class="net.officefloor.example.ejborchestration.LoginAction">
			<result name="logout">/logout.jsp</result>
			<result name="re-login">/login.jsp</result>
			<result name="success" type="chain">index</result>
		</action>
		<action name="logout"
			class="net.officefloor.example.ejborchestration.LogoutAction">
			<result name="success" type="chain">index</result>
		</action>
		<action name="createCustomer"
			class="net.officefloor.example.ejborchestration.CreateCustomerAction">
			<result name="re-create">/createCustomer.jsp</result>
			<result name="success" type="chain">index</result>
		</action>
		<action name="shoppingCart"
			class="net.officefloor.example.ejborchestration.ShowShoppingCartAction">
			<result name="success">/shoppingCart.jsp</result>
		</action>
	</package>

</struts>