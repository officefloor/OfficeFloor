<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         id="net.officefloor.editors"
         name="Editors"
         point="org.eclipse.ui.editors">
      <editor
            class="net.officefloor.eclipse.desk.DeskEditor"
            default="true"
            extensions="desk"
            icon="icons/office.gif"
            id="net.officefloor.editors.desk"
            name="Desk Editor"/>
      <editor
            class="net.officefloor.eclipse.room.RoomEditor"
            default="true"
            extensions="room"
            icon="icons/office.gif"
            id="net.officefloor.editors.room"
            name="Room Editor"/>
      <editor
            class="net.officefloor.eclipse.office.OfficeEditor"
            default="true"
            extensions="office"
            icon="icons/office.gif"
            id="net.officefloor.editors.office"
            name="Office Editor"/>
      <editor
            class="net.officefloor.eclipse.officefloor.OfficeFloorEditor"
            default="true"
            extensions="officefloor"
            icon="icons/office.gif"
            id="net.officefloor.editors.officefloor"
            name="OfficeFloor Editor"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="net.officefloor.eclipse"
            name="Office Floor">
      </category>
      <wizard
            category="net.officefloor.eclipse"
            class="net.officefloor.eclipse.wizards.DeskNewWizard"
            icon="icons/office.gif"
            id="net.officefloor.eclipse.wizards.DeskNewWizard"
            name="Desk">
      </wizard>
      <wizard
            category="net.officefloor.eclipse"
            class="net.officefloor.eclipse.wizards.RoomNewWizard"
            icon="icons/office.gif"
            id="net.officefloor.eclipse.wizards.RoomNewWizard"
            name="Room">
      </wizard>
      <wizard
            category="net.officefloor.eclipse"
            class="net.officefloor.eclipse.wizards.OfficeNewWizard"
            icon="icons/office.gif"
            id="net.officefloor.eclipse.wizards.OfficeNewWizard"
            name="Office">
      </wizard>
   </extension>

</plugin>
