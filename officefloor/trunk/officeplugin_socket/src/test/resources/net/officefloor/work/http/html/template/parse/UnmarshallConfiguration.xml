<unmarshall node="template"
	class="net.officefloor.work.http.html.template.parse.config.TemplateConfig">
	<object method="addSection" node="section"
		class="net.officefloor.work.http.html.template.parse.config.TemplateSectionConfig">
		<value method="setName" node="section@name" />
		<object method="addContent" node="static"
			class="net.officefloor.work.http.html.template.parse.config.StaticTemplateSectionContentConfig">
			<value method="setContent" node="content" />
		</object>
		<object method="addContent" node="reference"
			class="net.officefloor.work.http.html.template.parse.config.ReferenceTemplateSectionContentConfig">
			<value method="setKey" node="reference@key" />
		</object>
	</object>
</unmarshall>
