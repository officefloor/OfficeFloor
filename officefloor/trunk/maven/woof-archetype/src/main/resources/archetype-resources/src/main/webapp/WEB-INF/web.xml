<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<!-- Content only used by JEE container. -->
	<!-- When running WoOF outside a JEE container this is only a marker file 
		for a directory of web resources. -->

	<display-name>WoOF Application</display-name>
	<description>Generated from WoOF archetype</description>

	<filter>
		<filter-name>WoOF</filter-name>
		<filter-class>net.officefloor.plugin.woof.servlet.WoofServletFilter
		</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>WoOF</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

</web-app>