<context>
    <office-floor>
        <teams>
            <team name="TEAM" source="ONE_PERSON" />
        </teams>
        <offices>
            <office name="OFFICE" source="OFFICE" location="office">
                <team name="OFFICE_TEAM">
                    <office-floor-team name="TEAM" />
                </team>
            </office>
        </offices>
    </office-floor>
    <office>
        <teams>
            <team name="OFFICE_TEAM" source="ONE_PERSON" />
        </teams>
        <administrators>
            <administrator name="ADMIN" source="CLASS">
                <properties>
                    <property name="class.name" value="${testcase}$SimpleAdmin" />
                </properties>
                <team name="OFFICE_TEAM" />
                <duty name="duty" />
            </administrator>
        </administrators>
        <sections>
            <section name="DESK" source="DESK" location="desk">
                <responsibility name="RESPONSIBILITY">
                    <team name="OFFICE_TEAM" />
                </responsibility>
                <sub-section>
                    <task name="TASK">
                        <pre-administer administrator="ADMIN"
                            duty="duty" />
                    </task>
                </sub-section>
            </section>
        </sections>
    </office>
    <desk>
        <works>
            <work name="WORK" source="CLASS">
                <properties>
                    <property name="class.name" value="${testcase}$SimpleWork" />
                </properties>
                <work-task name="task" />
            </work>
        </works>
        <tasks>
            <task name="TASK" work="WORK" task="task" public="false" />
        </tasks>
    </desk>
</context>