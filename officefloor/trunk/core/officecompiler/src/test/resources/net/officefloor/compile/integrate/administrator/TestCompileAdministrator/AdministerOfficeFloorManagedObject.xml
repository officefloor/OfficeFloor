<context>
    <office-floor>
        <managed-object-sources>
            <managed-object-source name="MANAGED_OBJECT_SOURCE"
                source="CLASS">
                <properties>
                    <property name="class.name"
                        value="${testcase}$SimpleManagedObject" />
                </properties>
                <managing-office name="OFFICE" />
            </managed-object-source>
        </managed-object-sources>
        <managed-objects>
            <managed-object name="MANAGED_OBJECT" scope="THREAD">
                <managed-object-source name="MANAGED_OBJECT_SOURCE" />
            </managed-object>
        </managed-objects>
        <teams>
            <team name="TEAM" source="ONE_PERSON" />
        </teams>
        <offices>
            <office name="OFFICE" source="OFFICE" location="office">
                <team name="OFFICE_TEAM">
                    <office-floor-team name="TEAM" />
                </team>
                <object name="OBJECT">
                    <managed-object name="MANAGED_OBJECT" />
                </object>
            </office>
        </offices>
    </office-floor>
    <office>
        <external-managed-objects>
            <external-managed-object name="OBJECT"
                object-type="java.sql.Connection">
                <administer administrator="ADMIN" order="1" />
            </external-managed-object>
        </external-managed-objects>
        <teams>
            <team name="OFFICE_TEAM" source="ONE_PERSON" />
        </teams>
        <administrators>
            <administrator name="ADMIN" source="CLASS">
                <properties>
                    <property name="class.name" value="${testcase}$SimpleAdmin" />
                </properties>
                <team name="OFFICE_TEAM" />
            </administrator>
        </administrators>
    </office>
</context>