<office-floor>
	<managed-object-sources>
		<managed-object-source id="MO-ID"
			source="net.officefloor.mo.TestManagedObjectSource">
			<managing-office name="office" />
			<properties>
				<property name="mo prop name" value="mo prop value" />
			</properties>
			<dependencies>
				<dependency key="DEPENDENCY_KEY"
					type="java.sql.Connection" />
			</dependencies>
			<handlers>
				<handler key="HANDLER_KEY"
					key-class="net.example.HandlerKey"
					type="net.example.ExampleHandler">
					<handler-instance
						loader="net.officefloor.handler.example.ExampleHandlerLoader"
						configuration="net.example.ExampleHandler">
						<link-process id="0" work="work" task="task" />
						<link-process id="1">
							<task work="work.name" task="task.name" />
						</link-process>
					</handler-instance>
				</handler>
			</handlers>
			<tasks>
				<task work="work" task="task" team="MO-TEAM-NAME">
					<flows>
						<flow id="flow" work="work.name"
							task="task.name" />
					</flows>
				</task>
			</tasks>
			<teams>
				<team name="MO-TEAM-NAME">
					<team-instance name="TEAM-ID" />
				</team>
			</teams>
		</managed-object-source>
	</managed-object-sources>
	<team-instances>
		<team-instance id="TEAM-ID"
			team-factory="net.officefloor.team.TestTeamFactory">
			<properties>
				<property name="team prop name" value="team prop value" />
			</properties>
		</team-instance>
	</team-instances>
	<offices>
		<office id="OFFICE" name="office" x="100" y="20">
			<managed-objects>
				<managed-object name="MO-NAME" scope="process">
					<managed-object-source id="MO-ID" />
				</managed-object>
			</managed-objects>
			<teams>
				<team name="TEAM-NAME">
					<team-instance id="TEAM-ID" />
				</team>
			</teams>
			<tasks>
				<task work="work.name" task="task.name" />
			</tasks>
		</office>
	</offices>
</office-floor>