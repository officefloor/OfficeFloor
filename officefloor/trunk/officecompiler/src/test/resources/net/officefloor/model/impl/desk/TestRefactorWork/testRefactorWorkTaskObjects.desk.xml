                    object-type="java.lang.String">
                    <external-managed-object
                        name="MO_RENAME" />
                </task-object>
                <task-object name="REORDER_B" object-type="java.lang.Double">
                    <external-managed-object
                        name="MO_REORDER_B" />
                </task-object>
                <task-object name="REORDER_A" object-type="java.lang.Float">
                    <external-managed-object
                        name="MO_REORDER_A" />
                </task-object>
            </work-task>
        </work>
    </works>
