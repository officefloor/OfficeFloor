<desk>
	<external-managed-objects>
		<external-managed-object name="mo"
			object-type="java.lang.String" x="100" y="101" />
	</external-managed-objects>

    <managed-object-sources>
        <managed-object-source name="MANAGED_OBJECT_SOURCE"
            source="net.example.ExampleManagedObjectSource" object-type="net.orm.Session"
            x="200" y="201">
            <properties>
                <property name="MO_ONE" value="VALUE_ONE" />
                <property name="MO_TWO" value="VALUE_TWO" />
            </properties>
<!--
            <flow name="FLOW" argument-type="java.lang.Integer">
                <office-input office="OFFICE" section="SECTION" input="INPUT" />
            </flow>
-->
        </managed-object-source>
    </managed-object-sources>

    <managed-objects>
        <managed-object name="MANAGED_OBJECT" scope="THREAD"
            x="300" y="301">
            <managed-object-source name="MANAGED_OBJECT_SOURCE" />
<!--
            <dependency name="DEPENDENCY" object-type="java.sql.Connection">
                <managed-object name="MANAGED_OBJECT_TWO" />
            </dependency>
-->
        </managed-object>
    </managed-objects>

	<external-flows>
		<external-flow name="flow" argument-type="java.lang.Object"
			x="400" y="401" />
		<external-flow name="escalation" argument-type="java.lang.Throwable"
			x="410" y="411" />
	</external-flows>

	<works>
		<work name="work" source="net.example.ExampleWorkSource" x="500"
			y="501">
			<properties>
				<property name="property.one" value="VALUE_ONE" />
				<property name="property.two" value="VALUE_TWO" />
			</properties>
			<initial-task name="taskOne" />
			<work-task name="workTaskOne">
				<task-object name="ONE" key="ONE" object-type="java.lang.String">
					<external-managed-object name="mo" />
				</task-object>
			</work-task>
			<work-task name="workTaskTwo">
				<task-object name="0" object-type="java.lang.Integer"
					parameter="true" /> <!-- may not key -->
				<task-object name="1" object-type="java.lang.String">
					<external-managed-object name="mo" />
				</task-object>
				<task-object name="2" object-type="net.orm.Session">
				    <managed-object name="MANAGED_OBJECT" />
				</task-object>
			</work-task>
			<work-task name="workTaskThree">
				<task-object name="parameter" object-type="java.lang.Throwable"
					parameter="true" />
			</work-task>
			<work-task name="workTaskFour" /> <!-- May not require objects -->
		</work>
	</works>

	<tasks>
		<task name="taskOne" public="true" work="work" task="workTaskOne"
			return-type="java.lang.Integer" x="600" y="601">
			<task-flow name="First" key="ONE" argument-type="java.lang.Double">
				<external-flow name="flow" link="SEQUENTIAL" />
			</task-flow>
			<task-flow name="Second" key="TWO" argument-type="java.lang.Integer">
				<task name="taskTwo" link="PARALLEL" />
			</task-flow>
			<task-flow name="Third" key="THREE"> <!-- argument-type may be null -->
				<task name="taskFour" link="ASYNCHRONOUS" />
			</task-flow>
			<next-task name="taskTwo" />
			<task-escalation escalation-type="java.io.IOException">
				<task name="taskThree" />
			</task-escalation>
			<task-escalation escalation-type="java.sql.SQLException">
				<external-flow name="escalation" />
			</task-escalation>
			<task-escalation escalation-type="java.lang.NullPointerException" />
		</task>
		<task name="taskTwo" public="false" work="work" task="workTaskTwo"
			x="610" y="611"> <!-- return-type may be null -->
			<next-external-flow name="flow" />
		</task>
		<task name="taskThree" public="false" work="work" task="workTaskThree"
			return-type="java.lang.Integer" x="620" y="621">
			<task-flow name="0" argument-type="java.lang.Integer"> <!-- key may be null -->
				<task name="taskTwo" link="PARALLEL" />
			</task-flow>
			<task-flow name="1" argument-type="java.lang.Double">
				<external-flow name="flow" link="SEQUENTIAL" />
			</task-flow>
		</task>
		<task name="taskFour" public="false" work="work" task="workTaskFour"
			x="630" y="631">
		</task>
	</tasks>

</desk>