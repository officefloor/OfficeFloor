<desk>
	<external-managed-objects>
		<external-managed-object name="mo"
			object-type="java.lang.String" />
	</external-managed-objects>
	<external-flows>
		<external-flow name="flow" />
	</external-flows>
	<external-escalations>
		<external-escalation name="escalation"
			escalation-type="java.sql.SQLException" />
	</external-escalations>
	<works>
		<work id="work"
			loader="net.officefloor.work.clazz.ClassWorkLoader"
			configuration="net.officefloor.mock.MockClass">
			<initial-flow-item id="1" />
			<task name="taskMethod">
				<task-object object-type="java.lang.String">
					<external-managed-object name="mo" />
				</task-object>
			</task>
			<task name="noLongerExists">
				<task-object object-type="java.lang.Integer">
					<external-managed-object name="mo" />
				</task-object>
				<task-object object-type="java.lang.String"
					parameter="true" />
			</task>
			<task name="anotherMethod" />
		</work>
	</works>
	<flow-items>
		<flow-item id="1" x="100" y="200" public="true" work="work"
			task="taskMethod">
			<next-external-flow name="flow" />
			<output id="0">
				<external-flow name="flow" link="Sequential" />
			</output>
			<output id="1">
				<flow-item id="2" link="Parallel" />
			</output>
			<output id="2">
				<flow-item id="3" link="Asynchronous" />
			</output>
			<escalation escalation-type="java.io.IOException">
				<flow-item id="2" />
			</escalation>
			<escalation
				escalation-type="java.lang.NullPointerException" />
		</flow-item>
		<flow-item id="2" x="50" y="500" public="false" work="work"
			task="anotherMethod">
		</flow-item>
		<flow-item id="3" x="10" y="20" public="false" work="work"
			task="noLongerExists">
			<next-flow id="1" />
			<output id="FIRST_FLOW">
				<external-flow name="flow" link="Asynchronous" />
			</output>
			<output id="SECOND_FLOW">
				<flow-item id="1" link="Parallel" />
			</output>
			<escalation escalation-type="java.sql.SQLException">
				<external-escalation name="escalation" />
			</escalation>
		</flow-item>
	</flow-items>
</desk>
