<context>
    <office-floor>
        <managed-objects>
            <managed-object name="MANAGED_OBJECT_SOURCE"
                source="CLASS">
                <properties>
                    <property name="class.name"
                        value="${testcase}$SimpleManagedObject" />
                </properties>
                <managing-office name="OFFICE" />
            </managed-object>
        </managed-objects>
        <offices>
            <office name="OFFICE" source="OFFICE" location="office">
                <object name="MANAGED_OBJECT">
                    <managed-object name="MANAGED_OBJECT_SOURCE" />
                </object>
            </office>
        </offices>
    </office-floor>
    <office />
</context>