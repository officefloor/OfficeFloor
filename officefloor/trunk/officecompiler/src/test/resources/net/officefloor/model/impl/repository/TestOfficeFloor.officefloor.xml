<office-floor>
	<managed-object-sources>
		<managed-object-source name="MANAGED_OBJECT_SOURCE"
			source="net.example.ExampleManagedObjectSource" object-type="net.orm.Session"
			x="100" y="101">
			<properties>
				<property name="MO_ONE" value="VALUE_ONE" />
				<property name="MO_TWO" value="VALUE_TWO" />
			</properties>
			<managing-office name="OFFICE" />
			<input-managed-object name="INPUT_MANAGED_OBJECT" />
			<input-dependency name="INPUT_DEPENDENCY"
				object-type="java.sql.Connection">
				<managed-object name="MANAGED_OBJECT_TWO" />
			</input-dependency>
			<flow name="FLOW" argument-type="java.lang.Integer">
				<office-input office="OFFICE" section="SECTION" input="INPUT" />
			</flow>
			<team name="MO_TEAM">
				<office-floor-team name="TEAM" />
			</team>
		</managed-object-source>
	</managed-object-sources>
	<input-managed-objects>
		<input-managed-object name="INPUT_MANAGED_OBJECT"
			object-type="net.orm.Session" x="200" y="201" />
	</input-managed-objects>
	<managed-objects>
		<managed-object name="MANAGED_OBJECT_ONE" scope="THREAD"
			x="300" y="301">
			<managed-object-source name="MANAGED_OBJECT_SOURCE" />
			<dependency name="DEPENDENCY" object-type="java.sql.Connection">
				<managed-object name="MANAGED_OBJECT_TWO" />
			</dependency>
		</managed-object>
		<managed-object name="MANAGED_OBJECT_TWO" scope="PROCESS"
			x="310" y="311">
			<managed-object-source name="MANAGED_OBJECT_SOURCE" />
		</managed-object>
	</managed-objects>
	<teams>
		<team name="TEAM" source="net.example.ExampleTeamSource" x="400"
			y="401">
			<properties>
				<property name="TEAM_ONE" value="VALUE_ONE" />
				<property name="TEAM_TWO" value="VALUE_TWO" />
			</properties>
		</team>
	</teams>
	<offices>
		<office name="OFFICE" source="net.example.ExampleOfficeSource"
			location="OFFICE_LOCATION" x="400" y="401">
			<properties>
				<property name="OFFICE_ONE" value="VALUE_ONE" />
				<property name="OFFICE_TWO" value="VALUE_TWO" />
			</properties>
			<object name="OBJECT_A" object-type="net.orm.Session">
				<managed-object name="MANAGED_OBJECT" />
			</object>
			<object name="OBJECT_B" object-type="net.orm.Session">
				<input-managed-object name="INPUT_MANAGED_OBJECT" />
			</object>
			<input section="SECTION" input="INPUT" parameter-type="java.lang.Integer" />
			<team name="OFFICE_TEAM">
				<office-floor-team name="TEAM" />
			</team>
		</office>
	</offices>
</office-floor>