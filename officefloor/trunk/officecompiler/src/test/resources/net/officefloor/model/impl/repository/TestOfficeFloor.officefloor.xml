<office-floor>
    <managed-objects>
        <managed-object name="MANAGED_OBJECT"
            source="net.example.ExampleManagedObjectSource" object-type="net.orm.Session"
            x="10" y="11">
            <properties>
                <property name="MO_ONE" value="VALUE_ONE" />
                <property name="MO_TWO" value="VALUE_TWO" />
            </properties>
            <dependency name="DEPENDENCY" object-type="java.sql.Connection" />
            <flow name="FLOW" argument-type="java.lang.Integer" />
            <team name="MO_TEAM" />
        </managed-object>
    </managed-objects>
    <teams>
        <team name="TEAM" source="net.example.ExampleTeamSource" x="20"
            y="21">
            <properties>
                <property name="TEAM_ONE" value="VALUE_ONE" />
                <property name="TEAM_TWO" value="VALUE_TWO" />
            </properties>
        </team>
    </teams>
    <offices>
        <office name="OFFICE" source="net.example.ExampleOfficeSource"
            location="OFFICE_LOCATION" x="30" y="31">
            <properties>
                <property name="OFFICE_ONE" value="VALUE_ONE" />
                <property name="OFFICE_TWO" value="VALUE_TWO" />
            </properties>
            <object name="OBJECT" object-type="net.orm.Session" />
            <input name="INPUT" parameter-type="java.lang.Integer" />
            <team name="OFFICE_TEAM">
                <office-floor-team name="TEAM" />
            </team>
        </office>
    </offices>
</office-floor>