<office>
	<teams>
		<team name="TEAM ONE" />
		<team name="TEAM TWO" />
	</teams>
	<managed-objects>
		<managed-object name="MO" object-type="java.lang.String"
			scope="process" />
	</managed-objects>
	<room id="Room.room.xml" name="OFFICE ROOM">
		<room id="SubRoomOne.room.xml" name="RoomA">
			<room id="SubSubRoom.room.xml" name="SubRoomA" />
			<desk id="SubDesk.desk.xml" name="SubDeskA">
				<flow-item id="1" work="WorkA" task="FI1">
					<team name="TEAM ONE" />
					<pre-admin administrator="1" duty="KeyOne" />
					<pre-admin administrator="1" duty="KeyTwo" />
				</flow-item>
				<flow-item id="2" work="WorkB" task="FI2">
					<team name="TEAM TWO" />
					<post-admin administrator="1" duty="KeyOne" />
					<post-admin administrator="1" duty="KeyTwo" />
				</flow-item>
			</desk>
		</room>
		<room id="SubRoomTwo.room.xml" name="RoomB" />
		<desk id="Desk.desk.xml" name="DeskC">
			<flow-item id="3" work="WorkC" task="FLOW ITEM ONE">
				<team name="TEAM ONE" />
				<pre-admin administrator="1" duty="KeyOne" />
				<post-admin administrator="1" duty="KeyTwo" />
			</flow-item>
			<flow-item id="4" work="WorkD" task="FLOW ITEM TWO" />
		</desk>
	</room>
	<administrators>
		<administrator id="1"
			source="net.officefloor.admin.TestAdminSource" x="100" y="200">
			<properties>
				<property name="prop name" value="prop value" />
			</properties>
			<managed-objects>
				<managed-object name="MO" />
			</managed-objects>
			<duties>
				<duty key="KeyOne"
					flow-keys="net.officefloor.admin.TestDutyOneFlowKeys">
					<flows>
						<flow key="FlowOne">
							<flow-item id="1" />
						</flow>
						<flow key="FlowTwo" />
					</flows>
				</duty>
				<duty key="KeyTwo" />
			</duties>
		</administrator>
	</administrators>
</office>