<unmarshall node="template"
	class="net.officefloor.plugin.socket.server.http.template.parse.TemplateConfig">
	<object method="addSection" node="section"
		class="net.officefloor.plugin.socket.server.http.template.parse.TemplateSectionConfig">
		<value method="setName" node="section@name" />
		<object method="addContent" node="static"
			class="net.officefloor.plugin.socket.server.http.template.parse.StaticTemplateSectionContentConfig">
			<value method="setContent" node="content" />
		</object>
		<object method="addContent" node="reference"
			class="net.officefloor.plugin.socket.server.http.template.parse.ReferenceTemplateSectionContentConfig">
			<value method="setKey" node="reference@key" />
		</object>
		<object method="addContent" node="link"
			class="net.officefloor.plugin.socket.server.http.template.parse.LinkTemplateSectionContentConfig">
			<value method="setName" node="link@name" />
		</object>
	</object>
</unmarshall>
