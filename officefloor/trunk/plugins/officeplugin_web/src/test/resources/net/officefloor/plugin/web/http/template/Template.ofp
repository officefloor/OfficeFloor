    <!-- {List} -->
    <tr>
        <td>${Name}</td>
        <td>${description}</td>
        <td>${Property}</td>
        <td>${property.value}</td>
    </tr>
    <!-- {Tail} -->
</table>
<form action="#{submit}">
    <input type="submit" />
</form>
</body>
</html>