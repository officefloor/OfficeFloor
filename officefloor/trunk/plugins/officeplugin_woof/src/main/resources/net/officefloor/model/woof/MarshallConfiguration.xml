<?xml version="1.0" encoding="UTF-8"?>
<marshall type="net.officefloor.model.woof.WoofModel" element="woof">

	<!-- TEMPLATES -->
	<collection method="getWoofTemplates" element="templates">
		<item type="net.officefloor.model.woof.WoofTemplateModel"
			element="template">
			<attributes>
				<attribute method="getWoofTemplateName" attribute="name" />
				<attribute method="getUri" attribute="uri" />
				<attribute method="getTemplatePath" attribute="path" />
				<attribute method="getTemplateClassName" attribute="class" />
				<attribute method="getX" attribute="x" />
				<attribute method="getY" attribute="y" />
			</attributes>
			<collection method="getOutputs">
				<item type="net.officefloor.model.woof.WoofTemplateOutputModel"
					element="output">
					<attributes>
						<attribute method="getWoofTemplateOutputName" attribute="name" />
						<attribute method="getArgumentType" attribute="argument-type" />
					</attributes>
					<object method="getWoofSectionInput" element="input">
						<attributes>
							<attribute method="getSectionName" attribute="section" />
							<attribute method="getInputName" attribute="input" />
						</attributes>
					</object>
					<object method="getWoofTemplate" element="template">
						<attributes>
							<attribute method="getTemplateName" attribute="name" />
						</attributes>
					</object>
					<object method="getWoofResource" element="resource">
						<attributes>
							<attribute method="getResourceName" attribute="name" />
						</attributes>
					</object>
				</item>
			</collection>
			<collection method="getExtensions">
				<item type="net.officefloor.model.woof.WoofTemplateExtensionModel"
					element="extension">
					<attributes>
						<attribute method="getExtensionClassName" attribute="class" />
					</attributes>
					<collection method="getProperties">
						<item type="net.officefloor.model.woof.PropertyModel"
							element="property">
							<attributes>
								<attribute method="getName" attribute="name" />
								<attribute method="getValue" attribute="value" />
							</attributes>
						</item>
					</collection>
				</item>
			</collection>
		</item>
	</collection>

	<!-- SECTIONS -->
	<collection method="getWoofSections" element="sections">
		<item type="net.officefloor.model.woof.WoofSectionModel" element="section">
			<attributes>
				<attribute method="getWoofSectionName" attribute="name" />
				<attribute method="getSectionSourceClassName" attribute="source" />
				<attribute method="getSectionLocation" attribute="location" />
				<attribute method="getX" attribute="x" />
				<attribute method="getY" attribute="y" />
			</attributes>
			<collection method="getProperties">
				<item type="net.officefloor.model.woof.PropertyModel" element="property">
					<attributes>
						<attribute method="getName" attribute="name" />
						<attribute method="getValue" attribute="value" />
					</attributes>
				</item>
			</collection>
			<collection method="getInputs">
				<item type="net.officefloor.model.woof.WoofSectionInputModel"
					element="input">
					<attributes>
						<attribute method="getWoofSectionInputName" attribute="name" />
						<attribute method="getParameterType" attribute="parameter-type" />
						<attribute method="getUri" attribute="uri" />
					</attributes>
				</item>
			</collection>
			<collection method="getOutputs">
				<item type="net.officefloor.model.woof.WoofSectionOutputModel"
					element="output">
					<attributes>
						<attribute method="getWoofSectionOutputName" attribute="name" />
						<attribute method="getArgumentType" attribute="argument-type" />
					</attributes>
					<object method="getWoofSectionInput" element="input">
						<attributes>
							<attribute method="getSectionName" attribute="section" />
							<attribute method="getInputName" attribute="input" />
						</attributes>
					</object>
					<object method="getWoofTemplate" element="template">
						<attributes>
							<attribute method="getTemplateName" attribute="name" />
						</attributes>
					</object>
					<object method="getWoofResource" element="resource">
						<attributes>
							<attribute method="getResourceName" attribute="name" />
						</attributes>
					</object>
				</item>
			</collection>
		</item>
	</collection>

	<!-- RESOURCES -->
	<collection method="getWoofResources" element="resources">
		<item type="net.officefloor.model.woof.WoofResourceModel"
			element="resource">
			<attributes>
				<attribute method="getWoofResourceName" attribute="name" />
				<attribute method="getResourcePath" attribute="path" />
				<attribute method="getX" attribute="x" />
				<attribute method="getY" attribute="y" />
			</attributes>
		</item>
	</collection>

	<!-- EXCEPTIONS -->
	<collection method="getWoofExceptions" element="exceptions">
		<item type="net.officefloor.model.woof.WoofExceptionModel"
			element="exception">
			<attributes>
				<attribute method="getClassName" attribute="class" />
				<attribute method="getX" attribute="x" />
				<attribute method="getY" attribute="y" />
			</attributes>
			<object method="getWoofSectionInput" element="input">
				<attributes>
					<attribute method="getSectionName" attribute="section" />
					<attribute method="getInputName" attribute="input" />
				</attributes>
			</object>
			<object method="getWoofTemplate" element="template">
				<attributes>
					<attribute method="getTemplateName" attribute="name" />
				</attributes>
			</object>
			<object method="getWoofResource" element="resource">
				<attributes>
					<attribute method="getResourceName" attribute="name" />
				</attributes>
			</object>
		</item>
	</collection>

</marshall>