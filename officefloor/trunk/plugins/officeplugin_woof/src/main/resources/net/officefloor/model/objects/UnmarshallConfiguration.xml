<?xml version="1.0" encoding="UTF-8"?>
<unmarshall node="services" class="net.officefloor.model.service.ServicesModel">

	<!-- MANAGED OBJECTS -->
	<object method="addServiceManagedObject" node="managed-object"
		class="net.officefloor.model.service.ServiceManagedObjectModel">
		<value method="setServiceManagedObjectName" node="managed-object@name" />
		<value method="setManagedObjectSourceClassName" node="managed-object@source" />
		<value method="setX" node="managed-object@x" />
		<value method="setY" node="managed-object@y" />
		<object method="addProperty" node="property"
			class="net.officefloor.model.service.PropertyModel">
			<value method="setName" node="property@name" />
			<value method="setValue" node="property@value" />
		</object>
		<object method="addPropertyFile" node="property-file"
			class="net.officefloor.model.service.PropertyFileModel">
			<value method="setPath" node="property-file@path" />
		</object>
		<object method="addObjectType" node="object-type"
			class="net.officefloor.model.service.ServiceManagedObjectTypeModel">
			<value method="setObjectType" node="object-type@type" />
		</object>
	</object>

	<!-- TEAMS -->
	<object method="addServiceTeam" node="team"
		class="net.officefloor.model.service.ServiceTeamModel">
		<value method="setServiceTeamName" node="team@name" />
		<value method="setTeamSourceClassName" node="team@source" />
		<value method="setX" node="team@x" />
		<value method="setY" node="team@y" />
		<object method="addProperty" node="property"
			class="net.officefloor.model.service.PropertyModel">
			<value method="setName" node="property@name" />
			<value method="setValue" node="property@value" />
		</object>
		<object method="addPropertyFile" node="property-file"
			class="net.officefloor.model.service.PropertyFileModel">
			<value method="setPath" node="property-file@path" />
		</object>
	</object>

</unmarshall>