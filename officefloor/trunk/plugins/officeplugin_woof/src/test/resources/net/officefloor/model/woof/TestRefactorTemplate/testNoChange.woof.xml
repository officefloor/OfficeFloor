<woof>

	<templates>
		<template name="template" uri="template" path="example/Template.html"
			class="net.example.LogicClass" secure="true" continue-rendering="true"
			extends="TEMPLATE_PARENT">
			<output name="OUTPUT_1" argument-type="java.lang.Integer">
				<input section="SECTION" input="INPUT" />
			</output>
			<output name="OUTPUT_2" argument-type="java.lang.String">
				<template name="TEMPLATE_LINK" />
			</output>
			<output name="OUTPUT_3">
				<resource name="RESOURCE" />
			</output>
			<output name="output" />
			<link name="LINK_1" secure="true" />
			<link name="LINK_2" secure="false" />
			<redirect method="OTHER" />
			<redirect method="POST" />
			<redirect method="PUT" />
			<extension class="COMET">
				<property name="manual.publish.method.name" value="manualPublish" />
			</extension>
			<extension class="GWT">
				<property name="gwt.module.path" value="net/example/template.gwt.xml" />
				<property name="gwt.async.service.interfaces"
					value="net.example.GwtServiceAsync,net.example.GwtAnotherAsync" />
			</extension>
		</template>
		<template name="TEMPLATE_LINK" path="example/Link.html" />
		<template name="TEMPLATE_PARENT" path="example/Parent.html" />
	</templates>

	<sections>
		<section name="SECTION" source="net.example.ExampleSectionSource"
			location="SECTION_LOCATION">
			<input name="INPUT" />
		</section>
	</sections>

	<resources>
		<resource name="RESOURCE" path="Example.html" />
	</resources>

</woof>