<woof>

	<templates>
		<template name="TEMPLATE" path="example/Template.ofp" class="net.example.LogicClass">
			<output name="OUTPUT_1">
				<input section="CHANGE" input="INPUT_CHANGE" />
			</output>
		</template>
	</templates>

	<sections>
		<section name="CHANGE" source="net.example.ChangeSectionSource"
			location="CHANGE_LOCATION">
			<property name="name.1" value="value.one" />
			<property name="name.two" value="value.2" />
			<input name="INPUT_CHANGE" parameter-type="java.lang.Double" />
			<output name="OUTPUT_A" argument-type="java.lang.Integer">
				<resource name="RESOURCE" />
			</output>
			<output name="OUTPUT_B" argument-type="java.lang.String">
				<input section="SECTION_LINK" input="INPUT" />
			</output>
			<output name="OUTPUT_C">
				<template name="TEMPLATE" />
			</output>
		</section>
		<section name="SECTION_LINK" source="net.example.LinkSectionSource"
			location="LINK_LOCATION">
			<input name="INPUT" />
		</section>
	</sections>

	<resources>
		<resource name="RESOURCE" path="Example.html" />
	</resources>

	<exceptions>
		<exception class="java.io.IOException">
			<input section="CHANGE" input="INPUT_CHANGE" />
		</exception>
	</exceptions>

	<starting>
		<start>
			<flow section="CHANGE" input="INPUT_CHANGE" />
		</start>
	</starting>

</woof>