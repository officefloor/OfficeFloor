<woof>

	<templates>
		<template name="add" uri="add" path="example/Add.html" class="net.example.AddClass"
			secure="true" continue-rendering="true" extends="TEMPLATE_PARENT">
			<output name="OUTPUT_1" argument-type="java.lang.Integer" />
			<output name="OUTPUT_2" argument-type="java.lang.String" />
			<output name="OUTPUT_3" />
			<link name="LINK_1" secure="true" />
			<link name="LINK_2" secure="false" />
			<redirect method="OTHER" />
			<redirect method="POST" />
			<extension
				class="net.officefloor.model.woof.MockNoChangeWoofTemplateExtensionSource">
				<property name="name" value="value" />
			</extension>
			<extension
				class="net.officefloor.model.woof.MockNoChangeWoofTemplateExtensionSource" />
			<extension
				class="net.officefloor.model.woof.MockChangeWoofTemplateExtensionSource">
				<property name="ONE" value="A" />
				<property name="TWO" value="B" />
			</extension>
		</template>
		<template name="TEMPLATE_PARENT" path="example/Parent.html" />
	</templates>

</woof>