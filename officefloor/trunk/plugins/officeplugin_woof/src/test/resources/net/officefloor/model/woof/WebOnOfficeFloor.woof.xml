<woof>

	<templates>
		<template name="TEMPLATE_A" uri="example" path="example/TemplateA.ofp"
			extends="TEMPLATE_B" class="net.example.ExampleClassA" secure="true"
			continue-rendering="true" x="300" y="301">
			<output name="OUTPUT_1" argument-type="java.lang.Integer">
				<input section="SECTION_A" input="INPUT_A" />
			</output>
			<output name="OUTPUT_2">
				<template name="TEMPLATE_B" />
			</output>
			<output name="OUTPUT_3">
				<resource name="RESOURCE" />
			</output>
			<output name="OUTPUT_4">
				<access input="Authenticate" />
			</output>
			<output name="OUTPUT_5">
				<inherit />
			</output>
			<output name="OUTPUT_6" />
			<link name="LINK_1" secure="true" />
			<link name="LINK_2" secure="false" />
			<redirect method="REDIRECT_POST" />
			<redirect method="REDIRECT_PUT" />
			<extension class="GWT">
				<property name="NAME.1" value="VALUE.1" />
				<property name="NAME.2" value="VALUE.2" />
			</extension>
			<extension class="net.example.Extension" />
		</template>
		<template name="TEMPLATE_B" uri="another" path="example/TemplateB.ofp"
			x="302" y="303" />
	</templates>

	<sections>
		<section name="SECTION_A" source="DESK" location="DESK_LOCATION"
			x="400" y="401">
			<property name="name.one" value="value.one" />
			<property name="name.two" value="value.two" />
			<input name="INPUT_A" parameter-type="java.lang.Integer" />
			<input name="INPUT_B" uri="example" />
			<output name="OUTPUT_A" argument-type="java.lang.String">
				<input section="SECTION_B" input="INPUT_1" />
			</output>
			<output name="OUTPUT_B">
				<template name="TEMPLATE_A" />
			</output>
			<output name="OUTPUT_C">
				<resource name="RESOURCE" />
			</output>
			<output name="OUTPUT_D">
				<access input="Authenticate" />
			</output>
			<output name="OUTPUT_E" />
		</section>
		<section name="SECTION_B" source="net.example.ExampleSectionSource"
			location="EXAMPLE_LOCATION" x="402" y="403">
			<input name="INPUT_1" />
		</section>
	</sections>

	<access source="net.example.HttpSecuritySource" timeout="2000"
		x="500" y="501">
		<property name="name.first" value="value.first" />
		<property name="name.second" value="value.second" />
		<input name="Authenticate" parameter-type="net.example.HttpCredentials" />
		<output name="OUTPUT_ONE" argument-type="java.lang.String">
			<input section="SECTION_B" input="INPUT_1" />
		</output>
		<output name="OUTPUT_TWO">
			<template name="TEMPLATE_A" />
		</output>
		<output name="OUTPUT_THREE">
			<resource name="RESOURCE" />
		</output>
		<output name="OUTPUT_FOUR" />
	</access>

	<governances>
		<governance name="GOVERNANCE" source="net.example.ExampleGovernanceSource"
			x="600" y="601">
			<property name="name.a" value="value.a" />
			<property name="name.b" value="value.b" />
			<governance-area x="610" y="611" width="620" height="621" />
			<governance-area x="630" y="631" width="640" height="641" />
		</governance>
	</governances>

	<resources>
		<resource name="RESOURCE" path="Example.html" x="700" y="701" />
	</resources>

	<exceptions>
		<exception class="java.lang.Exception" x="800" y="801">
			<input section="SECTION_A" input="INPUT_A" />
		</exception>
		<exception class="java.lang.RuntimeException" x="802" y="803">
			<template name="TEMPLATE_A" />
		</exception>
		<exception class="java.sql.SQLException" x="804" y="805">
			<resource name="RESOURCE" />
		</exception>
		<exception class="java.io.IOException" x="806" y="807" />
	</exceptions>

	<starting>
		<start x="900" y="901">
			<flow section="SECTION_A" input="INPUT_A" />
		</start>
	</starting>

</woof>