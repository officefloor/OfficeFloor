<woof>
    
    <templates>
        <template name="TEMPLATE_A" uri="example" path="WOOF/TemplateA.ofp"
         class="net.officefloor.plugin.woof.WoofLoaderTest$Template">
            <output name="OUTPUT_1" argument-type="java.lang.Integer">
                <input section="SECTION_A" input="INPUT_A"/>
            </output>
            <output name="OUTPUT_2">
                <template name="TEMPLATE_B"/>
            </output>
            <output name="OUTPUT_3">
                <resource name="RESOURCE"/>
            </output>
            <output name="OUTPUT_4"/>
        </template>
        <template name="TEMPLATE_B" path="WOOF/TemplateB.ofp"
         class="net.officefloor.plugin.woof.WoofLoaderTest$Template"/>
    </templates>
    
    <sections>
        <section name="SECTION_A" source="net.officefloor.plugin.section.clazz.ClassSectionSource"
         location="net.officefloor.plugin.woof.WoofLoaderTest$Section">
            <property name="name.one" value="value.one"/>
            <property name="name.two" value="value.two"/>
            <input name="INPUT_A" parameter-type="java.lang.Integer"/>
            <input name="INPUT_B" uri="example"/>
            <output name="OUTPUT_A" argument-type="java.lang.String">
                <input section="SECTION_B" input="INPUT_1"/>
            </output>
            <output name="OUTPUT_B">
                <template name="TEMPLATE_A"/>
            </output>
            <output name="OUTPUT_C">
                <resource name="RESOURCE"/>
            </output>
            <output name="OUTPUT_D"/>
        </section>
        <section name="SECTION_B" source="net.officefloor.plugin.section.clazz.ClassSectionSource"
         location="net.officefloor.plugin.woof.WoofLoaderTest$Section">
            <input name="INPUT_1"/>
        </section>
    </sections>
    
    <resources>
        <resource name="RESOURCE" path="Example.html"/>
    </resources>
    
    <governances>
    	<governance name="GOVERNANCE_A" source="net.officefloor.plugin.governance.clazz.ClassGovernanceSource">
    	</governance>
    	<governance name="GOVERNANCE_B" source="net.officefloor.plugin.governance.clazz.ClassGovernanceSource">
    	</governance>
    </governances>
    
    <exceptions>
        <exception class="java.lang.Exception">
            <input section="SECTION_A" input="INPUT_A"/>
        </exception>
        <exception class="java.lang.RuntimeException">
            <template name="TEMPLATE_A"/>
        </exception>
        <exception class="java.sql.SQLException">
            <resource name="RESOURCE"/>
        </exception>
        <exception class="java.io.IOException"/>
    </exceptions>
    
</woof>