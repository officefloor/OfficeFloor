<woof>
    
    <templates>
        <template name="TEMPLATE_A" uri="example" path="WOOF/TemplateA.ofp"
         class="net.officefloor.plugin.woof.WoofLoaderTest$Template" 
         content-type="text/html; charset=UTF-8" secure="true" x="20" y="20">
            <output name="OUTPUT_1" argument-type="java.lang.Integer">
                <input section="SECTION_A" input="INPUT_A"/>
            </output>
            <output name="OUTPUT_2">
                <template name="TEMPLATE_B"/>
            </output>
            <output name="OUTPUT_3">
            	<access input="Authenticate"/>
            </output>
            <output name="OUTPUT_4">
                <resource name="RESOURCE"/>
            </output>
            <output name="OUTPUT_5"/>
            <link name="LINK_1" secure="true"/>
            <link name="LINK_2" secure="false"/>
            <redirect method="REDIRECT_POST"/>
            <redirect method="REDIRECT_PUT"/>
        </template>
        <template name="TEMPLATE_B" uri="another" path="WOOF/TemplateB.ofp" />
    </templates>
    
    <sections>
        <section name="SECTION_A" source="net.officefloor.plugin.section.clazz.ClassSectionSource"
         location="net.officefloor.plugin.woof.WoofLoaderTest$Section" x="80" y="80">
            <property name="name.one" value="value.one"/>
            <property name="name.two" value="value.two"/>
            <input name="INPUT_A" parameter-type="java.lang.Integer"/>
            <input name="INPUT_B" uri="example"/>
            <output name="OUTPUT_A" argument-type="java.lang.String">
                <input section="SECTION_B" input="INPUT_1"/>
            </output>
            <output name="OUTPUT_B">
                <template name="TEMPLATE_A"/>
            </output>
            <output name="OUTPUT_C">
                <access input="Authenticate"/>
            </output>
            <output name="OUTPUT_D">
                <resource name="RESOURCE"/>
            </output>
            <output name="OUTPUT_E"/>
        </section>
        <section name="SECTION_B" source="CLASS"
         location="net.officefloor.plugin.woof.WoofLoaderTest$Section">
            <input name="INPUT_1"/>
        </section>
    </sections>
    
    <access source="net.officefloor.plugin.web.http.security.scheme.MockHttpSecuritySource"
     timeout="2000">
    	<property name="name.first" value="value.first"/>
    	<property name="name.second" value="value.second"/>
    	<input name="Authenticate" parameter-type="net.example.HttpCredentials"/>
    	<output name="OUTPUT_ONE">
    		<input section="SECTION_B" input="INPUT_1"/>
    	</output>
    	<output name="OUTPUT_TWO">
    		<template name="TEMPLATE_A"/>
    	</output>
    	<output name="OUTPUT_THREE">
    		<resource name="RESOURCE"/>
    	</output>
    </access>
    
    <resources>
        <resource name="RESOURCE" path="Example.html"/>
    </resources>
    
    <governances>
    	<governance name="GOVERNANCE_A" source="net.officefloor.plugin.governance.clazz.ClassGovernanceSource">
            <property name="name.a" value="value.a"/>
            <property name="name.b" value="value.b"/>
    		<governance-area x="10" y="10" width="30" height="30" />
    		<governance-area x="70" y="70" width="90" height="90" />
    	</governance>
    	<governance name="GOVERNANCE_B" source="CLASS" />
    </governances>
    
    <exceptions>
        <exception class="java.lang.Exception">
            <input section="SECTION_A" input="INPUT_A"/>
        </exception>
        <exception class="java.lang.RuntimeException">
            <template name="TEMPLATE_A"/>
        </exception>
        <exception class="java.sql.SQLException">
            <resource name="RESOURCE"/>
        </exception>
        <exception class="java.io.IOException"/>
    </exceptions>
    
    <starting>
        <start>
            <flow section="SECTION_A" input="INPUT_A" />
        </start>
        <start>
            <flow section="SECTION_B" input="INPUT_1" />
        </start>
    </starting>
    
</woof>