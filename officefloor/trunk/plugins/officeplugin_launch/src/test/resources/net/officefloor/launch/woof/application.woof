<woof>
	<templates>
		<template name="TEMPLATE_A" uri="templateA" path="Template.html"
			class="net.officefloor.launch.woof.MockTemplateLogic">
			<extension class="GWT">
				<property name="gwt.module.path"
					value="net/officefloor/launch/woof/template.gwt.xml" />
			</extension>
		</template>
		<template name="TEMPLATE_B" uri="templateB" path="Template.html"
			class="net.officefloor.launch.woof.MockTemplateLogic">
			<!-- No GWT for template -->
		</template>
		<template name="TEMPLATE_C" uri="templateC" path="Template.html"
			class="net.officefloor.launch.woof.MockTemplateLogic">
			<extension class="GWT">
				<!-- GWT module re-used -->
				<property name="gwt.module.path"
					value="net/officefloor/launch/woof/template.gwt.xml" />
			</extension>
		</template>
		<template name="TEMPLATE_D" uri="templateD" path="Template.html"
			class="net.officefloor.launch.woof.MockTemplateLogic">
			<extension class="GWT">
				<property name="gwt.module.path"
					value="net/officefloor/launch/woof/another.gwt.xml" />
			</extension>
		</template>
	</templates>
	<sections>
		<section name="SECTION_A" source="DESK" location="DESK_LOCATION">
			<input name="INPUT_NO_URI" parameter-type="java.lang.Integer" />
			<input name="INPUT" uri="sectionA" />
		</section>
		<section name="SECTION_B" source="DESK" location="DESK_LOCATION">
			<input name="INPUT_NO_URI" parameter-type="java.lang.Integer" />
			<input name="INPUT" uri="sectionB" />
		</section>
	</sections>
</woof>