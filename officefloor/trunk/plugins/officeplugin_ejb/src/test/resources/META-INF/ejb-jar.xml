<ejb-jar>
	<enterprise-beans>
		<session>
			<ejb-name>MissingOfficeFloorJndiName</ejb-name>
			<ejb-class>net.officefloor.plugin.ejb.OfficeFloorEjb</ejb-class>
		</session>
		<session>
			<ejb-name>MissingOfficeName</ejb-name>
			<ejb-class>net.officefloor.plugin.ejb.OfficeFloorEjb</ejb-class>
			<env-entry>
				<env-entry-name>officeFloorJndiName</env-entry-name>
				<env-entry-value>mock:test</env-entry-value>
			</env-entry>
		</session>
		<session>
			<ejb-name>MissingWorkName</ejb-name>
			<ejb-class>net.officefloor.plugin.ejb.OfficeFloorEjb</ejb-class>
			<env-entry>
				<env-entry-name>officeFloorJndiName</env-entry-name>
				<env-entry-value>mock:test</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>officeName</env-entry-name>
				<env-entry-value>OFFICE</env-entry-value>
			</env-entry>
		</session>
		<session>
			<ejb-name>Configured</ejb-name>
			<ejb-class>net.officefloor.plugin.ejb.OfficeFloorEjb</ejb-class>
			<env-entry>
				<env-entry-name>officeFloorJndiName</env-entry-name>
				<env-entry-value>mock:test</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>officeName</env-entry-name>
				<env-entry-value>OFFICE</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>workName</env-entry-name>
				<env-entry-value>WORK</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>