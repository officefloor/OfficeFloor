<verifications>
	<files>
		<file>
			<!-- Ensure releasing latest version -->
			<location>${basedir}/../latest/pom.xml</location>
			<contains><![CDATA[<artifactId>${pom.artifactId}</artifactId>(\s)*<version>${pom.version}</version>]]></contains>
		</file>

		<file>
			<!-- Ensure run correct version of demo -->
			<location>officedemo</location>
			<contains>OFFICE_DEMO_VERSION=${pom.version}</contains>
		</file>
	</files>
</verifications>