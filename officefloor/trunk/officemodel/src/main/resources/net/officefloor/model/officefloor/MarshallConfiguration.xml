<?xml version="1.0" encoding="UTF-8"?>
<marshall type="net.officefloor.model.officefloor.OfficeFloorModel"
    element="office-floor">
    <!-- MANAGED OBJECTS -->
    <collection method="getOfficeFloorManagedObjects"
        element="managed-objects">
        <item
            type="net.officefloor.model.officefloor.OfficeFloorManagedObjectModel"
            element="managed-object">
            <attributes>
                <attribute method="getOfficeFloorManagedObjectName"
                    attribute="name" />
                <attribute method="getManagedObjectSourceClassName"
                    attribute="source" />
                <attribute method="getObjectType" attribute="object-type" />
                <attribute method="getX" attribute="x" />
                <attribute method="getY" attribute="y" />
            </attributes>
            <collection method="getProperties" element="properties">
                <item type="net.officefloor.model.officefloor.PropertyModel"
                    element="property">
                    <attributes>
                        <attribute method="getName" attribute="name" />
                        <attribute method="getValue"
                            attribute="value" />
                    </attributes>
                </item>
            </collection>
            <collection method="getOfficeFloorManagedObjectDependencies">
                <item
                    type="net.officefloor.model.officefloor.OfficeFloorManagedObjectDependencyModel"
                    element="dependency">
                    <attributes>
                        <attribute
                            method="getOfficeFloorManagedObjectDependencyName"
                            attribute="name" />
                        <attribute method="getDependencyType"
                            attribute="object-type" />
                    </attributes>
                </item>
            </collection>
            <collection method="getOfficeFloorManagedObjectFlows">
                <item
                    type="net.officefloor.model.officefloor.OfficeFloorManagedObjectFlowModel"
                    element="flow">
                    <attributes>
                        <attribute method="getOfficeFloorManagedObjectFlowName"
                            attribute="name" />
                        <attribute method="getArgumentType"
                            attribute="argument-type" />
                    </attributes>
                </item>
            </collection>
            <collection method="getOfficeFloorManagedObjectTeams">
                <item
                    type="net.officefloor.model.officefloor.OfficeFloorManagedObjectTeamModel"
                    element="team">
                    <attributes>
                        <attribute method="getOfficeFloorManagedObjectTeamName"
                            attribute="name" />
                    </attributes>
                </item>
            </collection>
        </item>
    </collection>
    <!-- TEAMS -->
    <collection method="getOfficeFloorTeams" element="teams">
        <item type="net.officefloor.model.officefloor.OfficeFloorTeamModel"
            element="team">
            <attributes>
                <attribute method="getOfficeFloorTeamName"
                    attribute="name" />
                <attribute method="getTeamSourceClassName"
                    attribute="source" />
                <attribute method="getX" attribute="x" />
                <attribute method="getY" attribute="y" />
            </attributes>
            <collection method="getProperties" element="properties">
                <item type="net.officefloor.model.officefloor.PropertyModel"
                    element="property">
                    <attributes>
                        <attribute method="getName" attribute="name" />
                        <attribute method="getValue"
                            attribute="value" />
                    </attributes>
                </item>
            </collection>
        </item>
    </collection>
    <!-- OFFICES -->
    <collection method="getDeployedOffices" element="offices">
        <item type="net.officefloor.model.officefloor.DeployedOfficeModel"
            element="office">
            <attributes>
                <attribute method="getDeployedOfficeName"
                    attribute="name" />
                <attribute method="getOfficeSourceClassName"
                    attribute="source" />
                <attribute method="getOfficeLocation"
                    attribute="location" />
                <attribute method="getX" attribute="x" />
                <attribute method="getY" attribute="y" />
            </attributes>
            <collection method="getProperties" element="properties">
                <item type="net.officefloor.model.officefloor.PropertyModel"
                    element="property">
                    <attributes>
                        <attribute method="getName" attribute="name" />
                        <attribute method="getValue"
                            attribute="value" />
                    </attributes>
                </item>
            </collection>
            <collection method="getDeployedOfficeObjects">
                <item
                    type="net.officefloor.model.officefloor.DeployedOfficeObjectModel"
                    element="object">
                    <attributes>
                        <attribute method="getDeployedOfficeObjectName"
                            attribute="name" />
                        <attribute method="getObjectType"
                            attribute="object-type" />
                    </attributes>
                </item>
            </collection>
            <collection method="getDeployedOfficeInputs">
                <item
                    type="net.officefloor.model.officefloor.DeployedOfficeInputModel"
                    element="input">
                    <attributes>
                        <attribute method="getDeployedOfficeInputName"
                            attribute="name" />
                        <attribute method="getParameterType"
                            attribute="parameter-type" />
                    </attributes>
                </item>
            </collection>
            <collection method="getDeployedOfficeTeams">
                <item
                    type="net.officefloor.model.officefloor.DeployedOfficeTeamModel"
                    element="team">
                    <attributes>
                        <attribute method="getDeployedOfficeTeamName"
                            attribute="name" />
                    </attributes>
                </item>
            </collection>
        </item>
    </collection>
</marshall>
