<?xml version="1.0" encoding="UTF-8"?>
<marshall xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../plugin/xml/src/schema/TreeXmlMarshaller.xsd" type="net.officefloor.model.officefloor.OfficeFloorModel" element="office-floor">
	<!-- MANAGED OBJECTS -->
	<collection method="getManagedObjectSources" element="managed-object-sources">
		<item type="net.officefloor.model.officefloor.ManagedObjectSourceModel" element="managed-object-source">
			<attributes>
				<attribute method="getId" attribute="id"/>
				<attribute method="getSource" attribute="source"/>
			</attributes>
			<object method="getManagingOffice" element="managing-office">
				<attributes>
					<attribute method="getManagingOfficeName" attribute="name"/>
				</attributes>
			</object>
			<collection method="getProperties" element="properties">
				<item type="net.officefloor.model.officefloor.PropertyModel" element="property">
					<attributes>
						<attribute method="getName" attribute="name"/>
						<attribute method="getValue" attribute="value"/>
					</attributes>
				</item>
			</collection>
		</item>
	</collection>
	<!-- TEAMS -->
	<collection method="getTeams" element="team-instances">
		<item type="net.officefloor.model.officefloor.TeamModel" element="team-instance">
			<attributes>
				<attribute method="getId" attribute="id"/>
				<attribute method="getTeamFactory" attribute="team-factory"/>
			</attributes>
			<collection method="getProperties" element="properties">
				<item type="net.officefloor.model.officefloor.PropertyModel" element="property">
					<attributes>
						<attribute method="getName" attribute="name"/>
						<attribute method="getValue" attribute="value"/>
					</attributes>
				</item>
			</collection>
		</item>
	</collection>
	<!-- OFFICES -->
	<collection method="getOffices" element="offices">
		<item type="net.officefloor.model.officefloor.OfficeFloorOfficeModel" element="office">
			<attributes>
				<attribute method="getId" attribute="id"/>
				<attribute method="getX" attribute="x"/>
				<attribute method="getY" attribute="y"/>
			</attributes>
			<collection method="getManagedObjects" element="managed-objects">
				<item type="net.officefloor.model.officefloor.OfficeManagedObjectModel" element="managed-object">
					<attributes>
						<attribute method="getManagedObjectName" attribute="name"/>
						<attribute method="getScope" attribute="scope"/>
					</attributes>
					<object method="getManagedObjectSource" element="managed-object-source">
						<attributes>
							<attribute method="getManagedObjectSourceId" attribute="id"/>
						</attributes>
					</object>
				</item>
			</collection>
			<collection method="getTeams" element="teams">
				<item type="net.officefloor.model.officefloor.OfficeTeamModel" element="team">
					<attributes>
						<attribute method="getTeamName" attribute="name"/>
					</attributes>
					<object method="getTeam" element="team-instance">
						<attributes>
							<attribute method="getTeamId" attribute="id"/>
						</attributes>
					</object>
				</item>
			</collection>
		</item>
	</collection>
</marshall>
