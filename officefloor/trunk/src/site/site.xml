<?xml version="1.0" encoding="UTF-8"?>
<project>
	<bannerLeft>
		<src>./images/OfficeFloorBannerImage.png</src>
		<href>http://www.officefloor.net/</href>
	</bannerLeft>
	<poweredBy>
		<logo name="Donate"
			href="http://sourceforge.net/donate/index.php?group_id=156349" img="http://images.sourceforge.net/images/project-support.jpg" />
		<logo name="SourceForge" href="http://sourceforge.net/"
			img="http://sflogo.sourceforge.net/sflogo.php?group_id=156349&amp;type=1" />
