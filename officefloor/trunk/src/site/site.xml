	</bannerLeft>
	<poweredBy>
		<logo name="Donate"
			href="http://sourceforge.net/donate/index.php?group_id=156349" img="http://images.sourceforge.net/images/project-support.jpg" />
		<logo name="SourceForge" href="http://sourceforge.net/"
			img="http://sflogo.sourceforge.net/sflogo.php?group_id=156349&amp;type=1" />
		<logo name="OpenSourceScripts"
			href="http://www.opensourcescripts.com/dir/Java/Web_Hosting_Tools/officefloor_5524.html"
			img="http://www.opensourcescripts.com/icons/twi.jpg" alt="open source scripts" />
		<logo name="Maven" href="http://maven.apache.org/"
