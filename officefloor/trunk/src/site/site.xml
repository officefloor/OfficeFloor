<?xml version="1.0" encoding="UTF-8"?>
<project>
	<bannerLeft>
		<src>./images/OfficeFloorBannerImage.png</src>
		<href>http://www.officefloor.net/</href>
	</bannerLeft>
	<poweredBy>
		<logo name="SourceForge" href="http://sourceforge.net/"
			img="http://sflogo.sourceforge.net/sflogo.php?group_id=156349&amp;type=1" />
		<logo name="Maven" href="http://maven.apache.org/"
			img="http://maven.apache.org/images/logos/maven-feather.png" />
		<logo name="Eclipse" href="http://www.eclipse.org/"
			img="http://www.eclipse.org/artwork/eclipseready/images/rea_eclipse_pos_logo_fc_sm.jpg" />
		<logo name="Ohloh"
			href="http://www.ohloh.net/p/officefloor?ref=WidgetProjectThinBadge"
			img="http://www.ohloh.net/p/officefloor/widgets/project_thin_badge.gif" />
	</poweredBy>
	<publishDate position="right" />
	<version position="right" />
	<skin>
		<groupId>net.officefloor.maven</groupId>
		<artifactId>maven-officefloor-skin</artifactId>
		<version>${officefloor.skin.version}</version>
	</skin>
	<body>
		<head>
			<!-- shortcut icon -->
			<link rel="shortcut icon" href="./favicon.ico" />
			<!-- Search Engine details -->
			<meta name="keywords"
				content="job,based,architecture,visual,graphical,configuration,framework,server,inversion,of,control,2.0,dependency,injection,java,XML,SEDA" />
			<meta name="description"
				content="OfficeFloor is an inversion of control 2.0 implementation" />
			<!-- Syntax Highlighter -->
			<link type="text/css" rel="stylesheet" href="./css/shCore.css" />
			<link type="text/css" rel="stylesheet" href="./css/shThemeDefault.css" />
			<script type="text/javascript" src="./scripts/shCore.js"></script>
			<script type="text/javascript" src="./scripts/shAutoloader.js"></script>
			<script type="text/javascript" src="./scripts/shBrushJava.js"></script>
			<script type="text/javascript" src="./scripts/shBrushXml.js"></script>
			<script type="text/javascript" src="./scripts/shBrushPlain.js"></script>
			<script type="text/javascript">
				SyntaxHighlighter.defaults['toolbar'] = false;
				SyntaxHighlighter.all();
            </script>
			<!-- Piwik -->
			<script type="text/javascript">
				var pkBaseURL = (("https:" ==
				document.location.protocol) ?
				"https://apps.sourceforge.net/piwik/officefloor/" :
				"http://apps.sourceforge.net/piwik/officefloor/");
				document.write(unescape("%3Cscript src='" + pkBaseURL + "piwik.js'
				type='text/javascript'%3E%3C/script%3E"));
            </script>
			<script type="text/javascript">
				piwik_action_name = '';
				piwik_idsite = 1;
				piwik_url = pkBaseURL + "piwik.php";
				piwik_log(piwik_action_name,
				piwik_idsite, piwik_url);
            </script>
			<object>
				<noscript>
					<p>
						<img
							src="http://apps.sourceforge.net/piwik/officefloor/piwik.php?idsite=1"
							alt="piwik" />
					</p>
				</noscript>
			</object>
		</head>
		<menu name="${project.name}">
			<item name="Welcome" href="./index.html" />
			<item name="Principles" href="./principles.html" />
			<item name="Job Based Architecture" href="./core/officeframe/job-based-architecture.html" />
			<item name="Graphical Configuration" href="./eclipse/graphical-configuration.html" />
		</menu>
		<menu name="Start using">
			<item name="Eclipse" href="./eclipse/index.html" />
			<item name="Download" href="./download.html" />
			<item name="Examples" href="./examples/index.html" />
		</menu>
		<menu name="Technical">
			<item name="Architecture" href="./architecture.html" />
			<item name="Source Repository" href="./source-repository.html" />
			<item name="SourceForge Page" href="http://sourceforge.net/projects/officefloor" />
		</menu>
		<menu name="Help">
			<item name="Getting Help" href="./getting-help.html" />
			<item name="Forum"
				href="http://sourceforge.net/search/?group_id=156349&amp;type_of_search=forums" />
			<item name="Wiki" href="http://sourceforge.net/apps/trac/officefloor" />
			<item name="Issue Tracking" href="./issue-tracking.html" />
		</menu>
		<menu ref="modules" />
	</body>
</project>