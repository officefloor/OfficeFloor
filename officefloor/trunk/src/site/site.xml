	</poweredBy>
	<publishDate position="right" />
	<version position="right" />
	<skin>
		<groupId>net.officefloor.maven</groupId>
		<artifactId>maven-officefloor-skin</artifactId>
		<version>${officefloor.skin.version}</version>
	</skin>
	<body>
		<head>
			<!-- shortcut icon -->
			<link rel="shortcut icon" href="./favicon.ico" />
			<!-- Search Engine details -->
			<meta name="keywords"
				content="Java,IOC,inversion of control,DI,dependency injection,visual configuration,graphical configuration,application framework" />
			<meta name="description"
				content="OfficeFloor is an inversion of control 2.0 that enables dependency injection of the thread for running a method" />
			<!-- Syntax Highlighter -->
			<link type="text/css" rel="stylesheet" href="./css/shCore.css" />
			<link type="text/css" rel="stylesheet" href="./css/shThemeDefault.css" />
			<script type="text/javascript" src="./scripts/shCore.js"></script>
			<script type="text/javascript" src="./scripts/shAutoloader.js"></script>
			<script type="text/javascript" src="./scripts/shBrushJava.js"></script>
			<script type="text/javascript" src="./scripts/shBrushXml.js"></script>
			<script type="text/javascript" src="./scripts/shBrushPlain.js"></script>
			<script type="text/javascript">
				SyntaxHighlighter.defaults['toolbar'] = false;
				SyntaxHighlighter.all();
            </script>
			<!-- Piwik -->
			<script type="text/javascript">
				var pkBaseURL = (("https:" ==
				document.location.protocol) ?
				"https://apps.sourceforge.net/piwik/officefloor/" :
				"http://apps.sourceforge.net/piwik/officefloor/");
				document.write(unescape("%3Cscript src='" + pkBaseURL + "piwik.js'
				type='text/javascript'%3E%3C/script%3E"));
            </script>
			<script type="text/javascript">
				piwik_action_name = '';
				piwik_idsite = 1;
				piwik_url = pkBaseURL + "piwik.php";
				piwik_log(piwik_action_name,
				piwik_idsite, piwik_url);
            </script>
			<object>
				<noscript>
					<p>
						<img
							src="http://apps.sourceforge.net/piwik/officefloor/piwik.php?idsite=1"
							alt="piwik" />
					</p>
				</noscript>
			</object>
			<!-- Google Analytics -->
			<script type="text/javascript">
				var _gaq = _gaq || [];
				_gaq.push(['_setAccount',
				'UA-21610196-1']);
				_gaq.push(['_trackPageview']);

				(function() {
				var ga
				= document.createElement('script'); ga.type = 'text/javascript';
				ga.async = true;
				ga.src = ('https:' == document.location.protocol ?
				'https://ssl' :
				'http://www') + '.google-analytics.com/ga.js';
				var s =
				document.getElementsByTagName('script')[0];
				s.parentNode.insertBefore(ga, s);
				})();
			</script>
			<!-- Start of Woopra Code -->
			<script>
				var woo_settings = {idle_timeout:'300000',
				domain:'officefloor.net'};
				(function(){
				var wsc =
				document.createElement('script');
				wsc.src =
				document.location.protocol+'//static.woopra.com/js/woopra.js';
				wsc.type = 'text/javascript';
				wsc.async = true;
				var ssc =
				document.getElementsByTagName('script')[0];
				ssc.parentNode.insertBefore(wsc, ssc);
				})();
			</script>
		</head>
