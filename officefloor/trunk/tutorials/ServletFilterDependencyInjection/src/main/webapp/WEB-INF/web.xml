<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<filter>
		<filter-name>WoOF</filter-name>
		<filter-class>net.officefloor.example.servletfilterdependencyinjection.ExampleFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>WoOF</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>Mock</servlet-name>
		<servlet-class>net.officefloor.example.servletfilterdependencyinjection.MockHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Mock</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

</web-app>