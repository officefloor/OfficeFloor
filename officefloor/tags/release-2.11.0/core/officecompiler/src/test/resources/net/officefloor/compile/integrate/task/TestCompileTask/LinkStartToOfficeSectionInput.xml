<context>
	<office-floor>
		<teams>
			<team name="TEAM" source="ONE_PERSON" />
		</teams>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<team name="OFFICE_TEAM">
					<office-floor-team name="TEAM" />
				</team>
			</office>
		</offices>
	</office-floor>
	<office>
		<teams>
			<team name="OFFICE_TEAM" />
		</teams>
		<starting>
			<start name="START">
				<flow section="SECTION" input="INPUT" />
			</start>
		</starting>
		<sections>
			<section name="SECTION" source="DESK" location="desk">
				<input name="INPUT" />
				<responsibility name="RESPONSIBILITY">
					<team name="OFFICE_TEAM" />
				</responsibility>
			</section>
		</sections>
	</office>
	<desk>
		<works>
			<work name="WORK" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileTaskWork" />
				</properties>
				<work-task name="simpleTask" />
			</work>
		</works>
		<tasks>
			<task name="INPUT" work="WORK" task="simpleTask" public="true" />
		</tasks>
	</desk>
</context>