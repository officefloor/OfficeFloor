<context item-labels="true">
	<office-floor>
		<office-floor>
			<teams>
				<team name="TEAM" source="ONE_PERSON" />
			</teams>
			<offices>
				<office name="OFFICE" source="OFFICE" location="office">
					<team name="OFFICE_TEAM">
						<office-floor-team name="TEAM" />
					</team>
				</office>
			</offices>
		</office-floor>
	</office-floor>
	<office>
		<office>
			<teams>
				<team name="OFFICE_TEAM" />
			</teams>
			<governances>
				<governance name="GOVERNANCE" source="CLASS">
					<properties>
						<property name="class.name" value="${testcase}$SimpleGovernance" />
					</properties>
					<team name="OFFICE_TEAM" />
					<governance-area x="20" y="20" height="60" width="60" />
				</governance>
			</governances>
			<sections>
				<section name="GOVERNED_SECTION" source="SECTION" location="section"
					x="40" y="40">
					<responsibility name="RESPONSIBILITY">
						<team name="OFFICE_TEAM" />
					</responsibility>
				</section>
				<section name="NON_GOVERNED_DESK" source="DESK" location="non-governed-desk"
					x="100" y="100">
					<responsibility name="RESPONSIBILITY">
						<team name="OFFICE_TEAM" />
					</responsibility>
				</section>
			</sections>
		</office>
	</office>
	<section>
		<section>
			<section>
				<sub-sections>
					<sub-section name="GOVERNED_DESK" source="DESK" location="governed-desk">
					</sub-section>
				</sub-sections>
			</section>
		</section>
	</section>
	<governed-desk>
		<desk>
			<works>
				<work name="WORK" source="CLASS">
					<properties>
						<property name="class.name" value="${testcase}$SimpleWork" />
					</properties>
					<work-task name="simpleTask" />
				</work>
			</works>
			<tasks>
				<task name="TASK" work="WORK" task="simpleTask" />
			</tasks>
		</desk>
	</governed-desk>
	<non-governed-desk>
		<desk>
			<works>
				<work name="WORK" source="CLASS">
					<properties>
						<property name="class.name" value="${testcase}$SimpleWork" />
					</properties>
					<work-task name="simpleTask" />
				</work>
			</works>
			<tasks>
				<task name="TASK" work="WORK" task="simpleTask" />
			</tasks>
		</desk>
	</non-governed-desk>
</context>