<context>
    <office-floor>
        <managed-object-sources>
            <managed-object-source name="MANAGED_OBJECT_SOURCE"
                source="CLASS">
                <properties>
                    <property name="class.name"
                        value="${testcase}$CompileManagedObject" />
                </properties>
                <managing-office name="OFFICE" />
            </managed-object-source>
        </managed-object-sources>
        <managed-objects>
            <managed-object name="MANAGED_OBJECT" scope="THREAD">
                <managed-object-source name="MANAGED_OBJECT_SOURCE" />
            </managed-object>
        </managed-objects>
        <teams>
            <team name="TEAM" source="ONE_PERSON" />
        </teams>
        <offices>
            <office name="OFFICE" source="OFFICE" location="office">
                <object name="OFFICE_OBJECT">
                    <managed-object name="MANAGED_OBJECT" />
                </object>
                <team name="OFFICE_TEAM">
                    <office-floor-team name="TEAM" />
                </team>
            </office>
        </offices>
    </office-floor>
    <office>
        <external-managed-objects>
            <external-managed-object name="OFFICE_OBJECT"
                object-type="${testcase}$CompileManagedObject" />
        </external-managed-objects>
        <teams>
            <team name="OFFICE_TEAM" />
        </teams>
        <sections>
            <section name="SECTION" source="DESK" location="desk">
                <object name="OBJECT">
                    <external-managed-object
                        name="OFFICE_OBJECT" />
                </object>
                <responsibility name="RESPONSIBILITY">
                    <team name="OFFICE_TEAM" />
                </responsibility>
            </section>
        </sections>
    </office>
    <desk>
        <external-managed-objects>
            <external-managed-object name="OBJECT" />
        </external-managed-objects>
        <works>
            <work name="WORK" source="CLASS">
                <properties>
                    <property name="class.name"
                        value="${testcase}$CompileTaskWork" />
                </properties>
                <work-task name="objectTask">
                    <task-object name="${testcase}$CompileManagedObject"
                        parameter="false">
                        <external-managed-object
                            name="OBJECT" />
                    </task-object>
                </work-task>
            </work>
        </works>
        <tasks>
            <task name="TASK" work="WORK" task="objectTask" />
        </tasks>
    </desk>
</context>