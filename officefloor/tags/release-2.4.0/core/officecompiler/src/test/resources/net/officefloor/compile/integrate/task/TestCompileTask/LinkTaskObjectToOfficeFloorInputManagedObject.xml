<context>
	<office-floor>
		<managed-object-sources>
			<managed-object-source name="MANAGED_OBJECT_SOURCE"
				source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$InputManagedObject" />
				</properties>
				<managing-office name="OFFICE" />
				<input-managed-object name="INPUT_MANAGED_OBJECT" />
				<flow name="doProcess">
					<office-input office="OFFICE" section="SECTION" input="TASK" />
				</flow>
			</managed-object-source>
		</managed-object-sources>
		<input-managed-objects>
			<input-managed-object name="INPUT_MANAGED_OBJECT" />
		</input-managed-objects>
		<teams>
			<team name="TEAM" source="ONE_PERSON" />
		</teams>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<input section="SECTION" input="TASK" />
				<object name="OFFICE_OBJECT">
					<input-managed-object name="INPUT_MANAGED_OBJECT" />
				</object>
				<team name="OFFICE_TEAM">
					<office-floor-team name="TEAM" />
				</team>
			</office>
		</offices>
	</office-floor>
	<office>
		<external-managed-objects>
			<external-managed-object name="OFFICE_OBJECT"
				object-type="${testcase}$InputManagedObject" />
		</external-managed-objects>
		<teams>
			<team name="OFFICE_TEAM" />
		</teams>
		<sections>
			<section name="SECTION" source="DESK" location="desk">
				<input name="TASK" />
				<object name="OBJECT">
					<external-managed-object name="OFFICE_OBJECT" />
				</object>
				<responsibility name="RESPONSIBILITY">
					<team name="OFFICE_TEAM" />
				</responsibility>
			</section>
		</sections>
	</office>
	<desk>
		<external-managed-objects>
			<external-managed-object name="OBJECT" />
		</external-managed-objects>
		<works>
			<work name="WORK" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileTaskWork" />
				</properties>
				<work-task name="inputObjectTask">
					<task-object name="${testcase}$InputManagedObject" parameter="false">
						<external-managed-object name="OBJECT" />
					</task-object>
				</work-task>
			</work>
		</works>
		<tasks>
			<task name="TASK" work="WORK" task="inputObjectTask" public="true" />
		</tasks>
	</desk>
</context>