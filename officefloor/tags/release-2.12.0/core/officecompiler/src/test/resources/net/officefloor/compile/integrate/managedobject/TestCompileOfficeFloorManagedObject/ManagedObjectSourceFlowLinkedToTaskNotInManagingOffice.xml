<context item-labels="true">
	<office-floor>
		<office-floor>
			<managed-object-sources>
				<managed-object-source name="MANAGED_OBJECT_SOURCE"
					source="CLASS">
					<properties>
						<property name="class.name" value="${testcase}$ProcessManagedObject" />
					</properties>
					<managing-office name="MANAGING_OFFICE" />
					<input-managed-object name="INPUT_MO" />
					<flow name="doProcess">
						<office-input office="OFFICE_WITH_TASK" section="SECTION"
							input="INPUT" />
					</flow>
				</managed-object-source>
			</managed-object-sources>
			<input-managed-objects>
				<input-managed-object name="INPUT_MO" />
			</input-managed-objects>
			<teams>
				<team name="TEAM" source="ONE_PERSON" />
			</teams>
			<offices>
				<office name="MANAGING_OFFICE" source="OFFICE" location="managing-office" />
				<office name="OFFICE_WITH_TASK" source="OFFICE" location="office-with-task">
					<input section="SECTION" input="INPUT" />
					<team name="OFFICE_TEAM">
						<office-floor-team name="TEAM" />
					</team>
				</office>
			</offices>
		</office-floor>
	</office-floor>
	<managing-office>
		<office />
	</managing-office>
	<office-with-task>
		<office>
			<teams>
				<team name="OFFICE_TEAM" />
			</teams>
			<sections>
				<section name="SECTION" source="DESK" location="desk">
					<input name="INPUT" />
					<responsibility name="RESPONSIBILITY">
						<team name="OFFICE_TEAM" />
					</responsibility>
				</section>
			</sections>
		</office>
	</office-with-task>
	<desk>
		<desk>
			<works>
				<work name="WORK" source="CLASS">
					<properties>
						<property name="class.name" value="${testcase}$ProcessWork" />
					</properties>
					<work-task name="process">
						<task-object name="java.lang.Integer" parameter="true" />
					</work-task>
				</work>
			</works>
			<tasks>
				<task name="INPUT" work="WORK" task="process" public="true" />
			</tasks>
		</desk>
	</desk>
</context>