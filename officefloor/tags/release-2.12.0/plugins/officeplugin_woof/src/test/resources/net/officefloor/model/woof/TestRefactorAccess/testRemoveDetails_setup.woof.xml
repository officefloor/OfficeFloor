<woof>

	<templates>
		<template name="TEMPLATE" path="example/Template.ofp" class="net.example.LogicClass">
			<output name="OUTPUT">
				<access input="Authenticate" />
			</output>
		</template>
	</templates>

	<sections>
		<section name="SECTION" source="net.example.ExampleSectionSource"
			location="SECTION_LOCATION">
			<input name="INPUT" />
			<output name="OUTPUT">
				<access input="Authenticate" />
			</output>
		</section>
	</sections>

	<access source="net.example.HttpSecuritySource" timeout="4000">
		<property name="name.one" value="value.one" />
		<property name="name.two" value="value.two" />
		<input name="Authenticate"
			parameter-type="net.officefloor.plugin.web.http.security.HttpCredentials" />
		<output name="OUTPUT_A" argument-type="java.lang.Integer">
			<input section="SECTION" input="INPUT" />
		</output>
		<output name="OUTPUT_B" argument-type="java.lang.String">
			<template name="TEMPLATE" />
		</output>
		<output name="OUTPUT_C">
			<resource name="RESOURCE" />
		</output>
	</access>

	<resources>
		<resource name="RESOURCE" path="Example.html" />
	</resources>

</woof>