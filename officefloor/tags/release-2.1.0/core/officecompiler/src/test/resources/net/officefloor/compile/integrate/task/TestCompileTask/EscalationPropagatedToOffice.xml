<context>
    <office-floor>
        <teams>
            <team name="TEAM" source="ONE_PERSON" />
        </teams>
        <offices>
            <office name="OFFICE" source="OFFICE" location="office">
                <team name="OFFICE_TEAM">
                    <office-floor-team name="TEAM" />
                </team>
            </office>
        </offices>
    </office-floor>
    <office>
        <teams>
            <team name="OFFICE_TEAM" />
        </teams>
        <sections>
            <section name="SECTION" source="DESK" location="desk">
                <output name="ESCALATION" escalation-only="true" />
                <responsibility name="RESPONSIBILITY">
                    <team name="OFFICE_TEAM" />
                </responsibility>
            </section>
        </sections>
    </office>
    <desk>
        <external-flows>
            <external-flow name="ESCALATION" />
        </external-flows>
        <works>
            <work name="WORK" source="CLASS">
                <properties>
                    <property name="class.name"
                        value="${testcase}$CompileTaskWork" />
                </properties>
                <work-task name="escalationTask" />
                <work-task name="simpleTask" />
            </work>
        </works>
        <tasks>
            <task name="TASK" work="WORK" task="escalationTask">
                <task-escalation escalation-type="java.lang.Exception">
                    <external-flow name="ESCALATION" />
                </task-escalation>
            </task>
        </tasks>
    </desk>
</context>