<woof>

	<templates>
		<template name="TEMPLATE" path="example/Template.ofp" class="net.example.LogicClass">
			<output name="OUTPUT_1" />
		</template>
	</templates>

	<sections>
		<section name="REMOVE" source="net.example.RemoveSectionSource"
			location="REMOVE_LOCATION">
		</section>
		<section name="SECTION_LINK" source="net.example.LinkSectionSource"
			location="LINK_LOCATION">
			<input name="INPUT" />
		</section>
	</sections>

	<resources>
		<resource name="RESOURCE" path="Example.html" />
	</resources>

	<exceptions>
		<exception class="java.io.IOException" />
	</exceptions>
	
	<starting>
	    <start />
	</starting>

</woof>