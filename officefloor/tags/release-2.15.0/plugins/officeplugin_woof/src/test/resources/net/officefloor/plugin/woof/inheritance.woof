<woof>

    <templates>
        
        <template name="PARENT_TEMPLATE" uri="parent" path="WOOF/Parent.ofp">
            <output name="OUTPUT_SECTION">
                <input section="SECTION" input="INPUT_1"/>
            </output>
            <output name="OUTPUT_TEMPLATE">
                <template name="TEMPLATE_1"/>
            </output>
            <output name="OUTPUT_ACCESS">
            	<access input="AUTHENTICATE_1"/>
            </output>
            <output name="OUTPUT_RESOURCE">
                <resource name="RESOURCE_1"/>
            </output>
            <link name="LINK_SECURE" secure="true"/>
            <link name="LINK_NON_SECURE" secure="false"/>
        </template>
        
        <template name="CHILD_TEMPLATE" uri="child" path="WOOF/Child.ofp"
          extends="PARENT_TEMPLATE">
            <link name="LINK_OTHER" secure="true"/>
        </template>

        <template name="GRAND_CHILD_TEMPLATE" uri="grandchild" path="WOOF/GrandChild.ofp"
          extends="CHILD_TEMPLATE">
            <output name="OUTPUT_SECTION">
                <input section="SECTION" input="INPUT_2"/>
            </output>
            <output name="OUTPUT_TEMPLATE">
                <template name="TEMPLATE_2"/>
            </output>
            <output name="OUTPUT_ACCESS">
            	<access input="AUTHENTICATE_2"/>
            </output>
            <output name="OUTPUT_RESOURCE">
                <resource name="RESOURCE_2"/>
            </output>
            <output name="OUTPUT_ANOTHER">
                <template name="TEMPLATE_3"/>
            </output>
            <link name="LINK_SECURE" secure="false"/>
            <link name="LINK_NON_SECURE" secure="true"/>
        </template>
        
        <template name="TEMPLATE_1" uri="one" path="WOOF/TemplateOne.ofp" />
        <template name="TEMPLATE_2" uri="two" path="WOOF/TemplateTwo.ofp" />
        <template name="TEMPLATE_3" uri="three" path="WOOF/TemplateThree.ofp" />
    </templates>
    
    <sections>
        <section name="SECTION" source="CLASS"
         location="net.officefloor.ExampleSection">
            <input name="INPUT_1"/>
            <input name="INPUT_2"/>
        </section>
    </sections>
    
    <access source="net.officefloor.plugin.web.http.security.scheme.MockHttpSecuritySource" timeout="2000">
    	<input name="AUTHENTICATE_1" />
    	<input name="AUTHENTICATE_2" />
    </access>
    
    <resources>
        <resource name="RESOURCE_1" path="ResourceOne.html"/>
        <resource name="RESOURCE_2" path="ResourceTwo.html"/>
    </resources>
    
</woof>