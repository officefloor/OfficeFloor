<woof>

	<templates>
		<template name="TEMPLATE" />
		<template name="TEMPLATE_LINK" />
	</templates>

	<sections>
		<section name="SECTION">
			<input name="INPUT" />
		</section>
		<section name="SECTION_LINK">
			<input name="INPUT" />
		</section>
	</sections>

	<access>
		<input name="Authenticate" />
		<output name="OUTPUT_A">
			<resource name="RESOURCE_LINK" />
		</output>
		<output name="OUTPUT_B">
			<template name="TEMPLATE" />
			<input section="SECTION" input="INPUT" />
			<resource name="RESOURCE" />
		</output>
	</access>

	<resources>
		<resource name="RESOURCE" />
		<resource name="RESOURCE_LINK" />
	</resources>

</woof>