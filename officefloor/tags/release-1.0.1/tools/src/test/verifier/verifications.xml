<verifications>
	<files>
		<!-- Ensure releasing latest version -->
		<file>
			<location>${basedir}/../eclipse/pom.xml</location>
			<contains><![CDATA[<dependencyManagement>[^;]*<artifactId>${project.artifactId}</artifactId>(\s)*<version>${project.version}</version>[^;]*</dependencyManagement>]]></contains>
		</file>
		
		<!-- Ensure all the modules inheriting latest -->
		<file>
			<location>officetool_demo/pom.xml</location>
			<contains><![CDATA[<version>${project.version}</version>(\s)*</parent>]]></contains>
		</file>
		<file>
			<location>officetool_filingcabinet/pom.xml</location>
			<contains><![CDATA[<version>${project.version}</version>(\s)*</parent>]]></contains>
		</file>
	</files>
</verifications>