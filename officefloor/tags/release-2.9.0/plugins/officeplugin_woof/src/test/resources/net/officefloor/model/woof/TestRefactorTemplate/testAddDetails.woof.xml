<woof>

	<templates>
		<template name="add" uri="add" path="example/Add.html" class="net.example.AddClass"
			secure="true" continue-rendering="true">
			<output name="OUTPUT_1" argument-type="java.lang.Integer" />
			<output name="OUTPUT_2" argument-type="java.lang.String" />
			<output name="OUTPUT_3" />
			<link name="LINK_1" secure="true" />
			<link name="LINK_2" secure="false" />
			<redirect method="OTHER" />
			<redirect method="POST" />
			<extension class="COMET">
				<property name="manual.publish.method.name" value="manualAdd" />
			</extension>
			<extension class="GWT">
				<property name="gwt.module.path" value="net/example/add.gwt.xml" />
				<property name="gwt.async.service.interfaces" value="net.example.GwtAddAsync" />
			</extension>
		</template>
	</templates>

</woof>