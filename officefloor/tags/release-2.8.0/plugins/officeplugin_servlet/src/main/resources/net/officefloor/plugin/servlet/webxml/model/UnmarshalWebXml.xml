<unmarshall xmlns="net.officefloor.plugin.xml.unmarshal"
	class="net.officefloor.plugin.servlet.webxml.model.WebAppModel" node="web-app">
	<value method="setVersion" node="web-app@version" />
	<value method="setDisplayName" node="display-name" />

	<object method="addContextParam" node="context-param"
		class="net.officefloor.plugin.servlet.webxml.model.ContextParamModel">
		<value method="setName" node="param-name" />
		<value method="setValue" node="param-value" />
	</object>

	<object method="addMimeMapping" node="mime-mapping"
		class="net.officefloor.plugin.servlet.webxml.model.MimeMappingModel">
		<value method="setExtension" node="extension" />
		<value method="setMimeType" node="mime-type" />
	</object>

	<object method="addServlet" node="servlet"
		class="net.officefloor.plugin.servlet.webxml.model.ServletModel">
		<value method="setServletName" node="servlet-name" />
		<value method="setServletClass" node="servlet-class" />
		<object method="addInitParam" node="init-param"
			class="net.officefloor.plugin.servlet.webxml.model.InitParamModel">
			<value method="setName" node="param-name" />
			<value method="setValue" node="param-value" />
		</object>
	</object>

	<object method="addServletMapping" node="servlet-mapping"
		class="net.officefloor.plugin.servlet.webxml.model.ServletMappingModel">
		<value method="setServletName" node="servlet-name" />
		<value method="addUrlPattern" node="url-pattern" />
	</object>

	<object method="addFilter" node="filter"
		class="net.officefloor.plugin.servlet.webxml.model.FilterModel">
		<value method="setFilterName" node="filter-name" />
		<value method="setFilterClass" node="filter-class" />
		<object method="addInitParam" node="init-param"
			class="net.officefloor.plugin.servlet.webxml.model.InitParamModel">
			<value method="setName" node="param-name" />
			<value method="setValue" node="param-value" />
		</object>
	</object>

	<object method="addFilterMapping" node="filter-mapping"
		class="net.officefloor.plugin.servlet.webxml.model.FilterMappingModel">
		<value method="setFilterName" node="filter-name" />
		<value method="addUrlPattern" node="url-pattern" />
		<value method="addServletName" node="servlet-name" />
		<value method="addDispatcher" node="dispatcher" />
	</object>

</unmarshall>