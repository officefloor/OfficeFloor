<context>
    <office-floor>
        <managed-object-sources>
            <managed-object-source name="MANAGED_OBJECT_SOURCE"
                source="CLASS">
                <properties>
                    <property name="class.name"
                        value="${testcase}$ProcessManagedObject" />
                </properties>
                <managing-office name="OFFICE"
                    process-bound-name="PROCESS_MO" />
                <flow name="doProcess">
                    <office-input office="OFFICE" section="SECTION"
                        input="INPUT" />
                </flow>
            </managed-object-source>
        </managed-object-sources>
        <teams>
            <team name="TEAM" source="ONE_PERSON" />
        </teams>
        <offices>
            <office name="OFFICE" source="OFFICE" location="office">
                <input section="SECTION" input="INPUT" />
                <team name="OFFICE_TEAM">
                    <office-floor-team name="TEAM" />
                </team>
            </office>
        </offices>
    </office-floor>
    <office>
        <teams>
            <team name="OFFICE_TEAM" />
        </teams>
        <sections>
            <section name="SECTION" source="DESK" location="desk">
                <input name="INPUT" />
                <responsibility name="RESPONSIBILITY">
                    <team name="OFFICE_TEAM" />
                </responsibility>
            </section>
        </sections>
    </office>
    <desk>
        <works>
            <work name="WORK" source="CLASS">
                <properties>
                    <property name="class.name" value="${testcase}$ProcessWork" />
                </properties>
                <work-task name="process">
                    <task-object name="Integer" parameter="true" />
                </work-task>
            </work>
        </works>
        <tasks>
            <task name="INPUT" work="WORK" task="process" public="true" />
        </tasks>
    </desk>
</context>