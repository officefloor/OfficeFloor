<context>
	<office-floor>
		<managed-object-sources>
			<managed-object-source name="SIMPLE_SOURCE"
				source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$SimpleManagedObject" />
				</properties>
				<managing-office name="OFFICE" />
			</managed-object-source>
		</managed-object-sources>
		<managed-objects>
			<managed-object name="SIMPLE" scope="PROCESS">
				<managed-object-source name="SIMPLE_SOURCE" />
			</managed-object>
		</managed-objects>
		<teams>
			<team name="TEAM" source="ONE_PERSON" />
		</teams>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<object name="EXTERNAL_MANAGED_OBJECT">
					<managed-object name="SIMPLE" />
				</object>
				<team name="OFFICE_TEAM">
					<office-floor-team name="TEAM" />
				</team>
			</office>
		</offices>
	</office-floor>
	<office>
		<external-managed-objects>
			<external-managed-object name="EXTERNAL_MANAGED_OBJECT"
				object-type="${testcase}$SimpleManagedObject" object-scope="PROCESS" />
		</external-managed-objects>
		<managed-object-sources>
			<managed-object-source name="INPUT_SOURCE"
				source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$InputManagedObject" />
				</properties>
				<input-dependency name="dependency">
					<external-managed-object name="EXTERNAL_MANAGED_OBJECT" />
				</input-dependency>
				<flow name="doProcess">
					<section-input section="SECTION" input="INPUT" />
				</flow>
			</managed-object-source>
		</managed-object-sources>
		<teams>
			<team name="OFFICE_TEAM" />
		</teams>
		<sections>
			<section name="SECTION" source="DESK" location="desk">
				<input name="INPUT" />
				<responsibility name="RESPONSIBILITY">
					<team name="OFFICE_TEAM" />
				</responsibility>
			</section>
		</sections>
	</office>
	<desk>
		<works>
			<work name="WORK" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$ProcessWork" />
				</properties>
				<work-task name="process">
					<task-object name="Integer" parameter="true" />
				</work-task>
			</work>
		</works>
		<tasks>
			<task name="INPUT" work="WORK" task="process" public="true" />
		</tasks>
	</desk>
</context>