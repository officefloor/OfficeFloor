<context item-labels="true">
	<office-floor>
		<office-floor>
			<teams>
				<team name="TEAM" source="ONE_PERSON" />
			</teams>
			<offices>
				<office name="OFFICE" source="OFFICE" location="office">
					<team name="OFFICE_TEAM">
						<office-floor-team name="TEAM" />
					</team>
				</office>
			</offices>
		</office-floor>
	</office-floor>
	<office>
		<office>
			<teams>
				<team name="OFFICE_TEAM" />
			</teams>
			<sections>
				<section name="DESK_A" source="DESK" location="deskA">
					<output name="EXTERNAL_FLOW">
						<section-input section="DESK_B" input="INPUT" />
					</output>
					<responsibility name="RESPONSIBILITY">
						<team name="OFFICE_TEAM" />
					</responsibility>
				</section>
				<section name="DESK_B" source="DESK" location="deskB">
					<input name="INPUT" />
					<responsibility name="RESPONSIBILITY">
						<team name="OFFICE_TEAM" />
					</responsibility>
				</section>
			</sections>
		</office>
	</office>
	<deskA>
		<desk>
			<external-flows>
				<external-flow name="EXTERNAL_FLOW" argument-type="java.lang.Integer" />
			</external-flows>
			<managed-object-sources>
				<managed-object-source name="MANAGED_OBJECT_SOURCE"
					source="CLASS">
					<properties>
						<property name="class.name" value="${testcase}$ProcessManagedObject" />
					</properties>
					<flow name="doProcess">
						<external-flow name="EXTERNAL_FLOW" />
					</flow>
				</managed-object-source>
			</managed-object-sources>
		</desk>
	</deskA>
	<deskB>
		<desk>
			<works>
				<work name="WORK" source="CLASS">
					<properties>
						<property name="class.name" value="${testcase}$ProcessWork" />
					</properties>
					<work-task name="process">
						<task-object name="Integer" parameter="true" />
					</work-task>
				</work>
			</works>
			<tasks>
				<task name="INPUT" work="WORK" task="process" public="true" />
			</tasks>
		</desk>
	</deskB>
</context>