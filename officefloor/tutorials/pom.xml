<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>net.officefloor</groupId>
		<artifactId>officefloor</artifactId>
		<version>${revision}</version>
	</parent>
	<groupId>net.officefloor.tutorial</groupId>
	<artifactId>tutorials</artifactId>
	<packaging>pom</packaging>
	<name>Tutorials</name>
	<description>Tutorials for OfficeFloor</description>
	<modules>
		<module>ActivityHttpServer</module>
		<module>AuthenticationHttpServer</module>
		<module>AwsSamHttpServer</module>
		<module>AzureHttpServer</module>
		<module>CatsHttpServer</module>
		<module>CorsHttpServer</module>
		<module>CosmosAsyncDbHttpServer</module>
		<module>CosmosDbHttpServer</module>
		<module>DatabaseHttpServer</module>
		<module>DeployHttpServer</module>
		<module>DiPojoHttpServer</module>
		<module>DynamicHttpServer</module>
		<module>DynamoDbHttpServer</module>
		<module>EnvironmentHttpServer</module>
		<module>ExceptionHttpServer</module>
		<module>FlywayHttpServer</module>
		<module>GoogleAppEngineHttpServer</module>
		<module>GoogleSigninHttpServer</module>
		<module>GraphicalConfigurationExample</module>
		<module>HttpServletHttpServer</module>
		<module>InheritHttpServer</module>
		<module>InteractiveHttpServer</module>
		<module>JavaScriptApp</module>
		<module>JavaScriptHttpServer</module>
		<module>JaxRsApp</module>
		<module>JaxRsHttpServer</module>
		<module>JaxRsWarHttpServer</module>
		<module>JwtAuthorityHttpServer</module>
		<module>JwtHttpServer</module>
		<module>JwtResourceHttpServer</module>
		<module>KotlinHttpServer</module>
		<module>LoggerHttpServer</module>
		<module>NavigateHttpServer</module>
		<module>ObjectifyHttpServer</module>
		<module>PayPalHttpServer</module>
		<module>PrototypeHttpServer</module>
		<module>R2dbcHttpServer</module>
		<module>RawHttpServer</module>
		<module>ReactorHttpServer</module>
		<module>RestHttpServer</module>
		<module>ScalaHttpServer</module>
		<module>SectionHttpServer</module>
		<module>SecurePageHttpServer</module>
		<module>SecureLinkHttpServer</module>
		<module>ServletHttpServer</module>
		<module>SessionHttpServer</module>
		<module>SpringWebFluxApp</module>
		<module>SpringWebFluxHttpServer</module>
		<module>SpringWebMvcApp</module>
		<module>SpringWebMvcHttpServer</module>
		<module>SpringHttpServer</module>
		<module>StartBeforeHttpServer</module>
		<module>StaticContentHttpServer</module>
		<module>SwaggerHttpServer</module>
		<module>TeamHttpServer</module>
		<module>TestHttpServer</module>
		<module>ThreadAffinityHttpServer</module>
		<module>TransactionHttpServer</module>
		<module>VariableHttpServer</module>
		<module>WarApp</module>
		<module>WarHttpServer</module>
		<module>ZioHttpServer</module>
	</modules>
	<build>
		<plugins>
			<plugin>
				<!-- Only license tutorial and verification. -->
				<!-- Developers may use tutorial code as seed. -->
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>license-maven-plugin</artifactId>
				<configuration>
					<roots>
						<root>src/site</root>
						<root>src/test/verifier</root>
					</roots>
				</configuration>
			</plugin>
			<plugin>
				<!-- Do not deploy tutorials. Not libraries for including in projects. -->
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.sonatype.plugins</groupId>
				<artifactId>nexus-staging-maven-plugin</artifactId>
				<configuration>
					<skipNexusStagingDeployMojo>true</skipNexusStagingDeployMojo>
				</configuration>
			</plugin>
			<plugin>
				<groupId>com.github.github</groupId>
				<artifactId>site-maven-plugin</artifactId>
				<executions>
					<execution>
						<id>github-repository</id>
						<configuration>
							<skip>true</skip>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.junit.vintage</groupId>
			<artifactId>junit-vintage-engine</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>