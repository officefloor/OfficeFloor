<woof>

	<http-continuations>
		<http-continuation path="/path" />
	</http-continuations>

	<http-inputs>
		<http-input path="/path" method="POST" />
	</http-inputs>

	<templates>
		<template path="/template" location="example/Template.ofp"
			class="net.example.LogicClass">
			<output name="OUTPUT" />
		</template>
	</templates>

	<sections>
		<section name="REMOVE" source="net.example.RemoveSectionSource"
			location="REMOVE_LOCATION">
		</section>
		<section name="SECTION_LINK" source="net.example.LinkSectionSource"
			location="LINK_LOCATION">
			<input name="INPUT" />
			<output name="OUTPUT" />
		</section>
	</sections>

	<securities>
		<security name="SECURITY" source="net.example.HttpSecuritySource">
			<output name="OUTPUT" />
		</security>
	</securities>

	<resources>
		<resource path="/resource.html" />
	</resources>

	<exceptions>
		<exception class="java.io.IOException" />
	</exceptions>

	<starts>
		<start />
	</starts>

</woof>