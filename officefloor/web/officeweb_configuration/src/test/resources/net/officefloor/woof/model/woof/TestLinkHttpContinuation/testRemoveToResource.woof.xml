<woof>

	<http-continuations>
		<http-continuation path="/pathA" />
		<http-continuation path="/pathB">
			<template path="/template" />
			<procedure name="PROCEDURE" />
			<section name="SECTION" input="INPUT" />
			<security name="SECURITY" />
			<redirect path="/pathA" />
		</http-continuation>
	</http-continuations>

	<templates>
		<template path="/template" />
		<template path="/templateLink" />
	</templates>
	
	<procedures>
		<procedure name="PROCEDURE" />
		<procedure name="PROCEDURE_LINK" />
	</procedures>

	<sections>
		<section name="SECTION">
			<input name="INPUT" />
		</section>
		<section name="SECTION_LINK">
			<input name="INPUT" />
		</section>
	</sections>

	<securities>
		<security name="SECURITY" />
		<security name="SECURITY_LINK" />
	</securities>

	<resources>
		<resource path="/resource.html" />
		<resource path="/resource.png" />
	</resources>

</woof>