<woof>

	<http-continuations>
		<http-continuation path="/path">
			<template path="/template" />
		</http-continuation>
	</http-continuations>

	<http-inputs>
		<http-input path="/path" method="POST">
			<template path="/template" />
		</http-input>
	</http-inputs>

	<templates>
		<template path="/parent" location="example/Parent.ofp">
			<output name="OUTPUT_INHERIT" />
		</template>
		<template path="/template" location="example/Remove.ofp">
			<extends template="/parent" />
		</template>
		<template path="/templateLink" location="example/Link.ofp">
			<output name="OUTPUT">
				<template path="/template" />
			</output>
		</template>
	</templates>

	<procedures>
		<procedure name="PROCEDURE" resource="resource" source="Class" procedure="method">
			<next>
				<template path="/template" />
			</next>
			<output name="OUTPUT">
				<template path="/template" />
			</output>
		</procedure>
	</procedures>

	<sections>
		<section name="SECTION" source="net.example.ExampleSectionSource" location="SECTION_LOCATION">
			<input name="INPUT" />
			<output name="OUTPUT">
				<template path="/template" />
			</output>
		</section>
	</sections>

	<securities>
		<security name="SECURITY" source="net.example.HttpSecuritySource">
			<output name="OUTPUT">
				<template path="/template" />
			</output>
		</security>
	</securities>

	<resources>
		<resource path="/resource.html" />
	</resources>

	<exceptions>
		<exception class="java.io.IOException">
			<template path="/template" />
		</exception>
	</exceptions>

</woof>