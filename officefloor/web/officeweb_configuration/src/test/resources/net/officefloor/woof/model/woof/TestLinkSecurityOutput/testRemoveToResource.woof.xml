<woof>

	<http-continuations>
		<http-continuation path="/path" />
		<http-continuation path="/pathLink" />
	</http-continuations>

	<templates>
		<template path="/template" />
		<template path="/templateLink" />
	</templates>

	<sections>
		<section name="SECTION">
			<input name="INPUT" />
		</section>
		<section name="SECTION_LINK">
			<input name="INPUT" />
		</section>
	</sections>

	<securities>
		<security name="A">
			<output name="OUTPUT" />
		</security>
		<security name="B">
			<output name="OUTPUT">
				<template path="/template" />
				<section name="SECTION" input="INPUT" />
				<security name="A" />
				<redirect path="/path" />
			</output>
		</security>
	</securities>

	<resources>
		<resource path="/resource.html" />
		<resource path="/resource.png" />
	</resources>

</woof>