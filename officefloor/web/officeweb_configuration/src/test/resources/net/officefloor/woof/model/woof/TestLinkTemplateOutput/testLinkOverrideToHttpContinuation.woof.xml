<woof>

	<http-continuations>
		<http-continuation path="/path" />
		<http-continuation path="/pathLink" />
	</http-continuations>

	<templates>
		<template path="/template">
			<output name="OUTPUT" />
		</template>
		<template path="/templateLink">
			<output name="OUTPUT">
				<redirect path="/pathLink" />
			</output>
		</template>
	</templates>

	<sections>
		<section name="SECTION">
			<input name="INPUT" />
		</section>
		<section name="SECTION_LINK">
			<input name="INPUT" />
		</section>
	</sections>

	<securities>
		<security name="SECURITY" />
		<security name="SECURITY_LINK" />
	</securities>

	<resources>
		<resource path="/resource.html" />
		<resource path="/resource.png" />
	</resources>

</woof>